<!DOCTYPE html>
<!DOCTYPE svg PUBLIC “-//W3C//DTD SVG 1.1//EN” “http://www.w3.org/Graphics/SVG/1.1/DTD/svg11.dtd”>
<?xml version="1.0" encoding="utf-8"?>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>{% block title %}Recognition of Prior Learning{% endblock %}</title>
    <!-- Bootstrap -->
    {% block head %} 
        {% set roles = app.security.getToken().getUser().getRoles() %}
        <link rel="stylesheet" href="{{ asset('public/css/bootstrap.min.css') }}" />
        <link rel="stylesheet" href="{{ asset('public/css/bootstrap-flex.css') }}" />
        <link rel="stylesheet" href="{{ asset('public/css/bootstrap-grid.css') }}" />
        <link rel="stylesheet" href="{{ asset('public/css/bootstrap-reboot.css') }}">              
        <link rel="stylesheet" href="{{ asset('public/css/jquery-ui.css') }}">
        <link rel="stylesheet" href="{{ asset('public/css/jquery-ui-timepicker-addon.css') }}">
        <link rel="stylesheet" href="{{ asset('public/css/pages/all/header.css') }}">
        {%if (is_mobile()) and ('ROLE_MANAGER' in roles or 'ROLE_SUPERADMIN' in roles) %}<link rel="stylesheet" href="{{ asset('public/css/pages/all/header-manager.css') }}">{%endif%}
        <link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800' rel='stylesheet' type='text/css'>
    {% endblock %}
    {% block stylesheets %}{% endblock %}  
        <link rel="stylesheet" href="{{ asset('public/css/pages/all/fixes.css') }}">
        <script type="text/javascript">
            var base_url = "{{ base_url }}";
            var amazon_link = "{{ amazon_link }}";
            var upload_path = "{{ asset('public/uploads/')}}";
        </script>    
</head>
<body>
    {% set roles = app.security.getToken().getUser().getRoles() %}
    {% set applicantStatus = app.security.getToken().getUser().getApplicantStatus() %}
    {% set page = app.request.attributes.get('_route') %}
    <main class="main_container">
        {% block navmenu %}
            {% set className =  (app.request.attributes.get('_route') in header_class_pages) ? 'header-border' : '' %}
                <header class="{{ className }}">
                    <section class="container">
                        <div class="logo">
                            {% if ('ROLE_APPLICANT' not in roles)  %}<a href="{{ app.request.baseUrl }}/dashboard">
                            {% else %}<a href="{{ app.request.baseUrl }}/qualifications">
                            {% endif %}
                            <img src="{{ asset('public/images/gqalogo.png') }}" alt="Get Qualified Australia Logo"/>
                            </a>
                        </div>
                        <div class="info-header hide">
                            <div class="portfolio"><strong class="title">Profile Name</strong><a href="#"><i class="material-icons clear">clear</i></a></div>
                            <div class="unit-info"><a href="#"><i class="material-icons btn-back">arrow_back</i></a><strong class="title">Unit info</strong></div>            
                        </div>
                        <nav>
                            <ul class="nav">
                                {% set muser = app.session.get('muser') %}
                                {% set suser = app.session.get('suser') %}
                                {% if ('ROLE_APPLICANT' not in roles)  %}
                                    <li><a class="{% if app.request.attributes.get('_route') == 'gq_aus_home_homepage' %} active {% endif %}" href="{{ app.request.baseUrl }}/dashboard"><i class="material-icons">assessment</i><span>Dashboard</span></a></li>
                                {% else %}
                                    <li><a class="{% if app.request.attributes.get('_route') == 'qualifications' or app.request.attributes.get('_route') == 'qualificationDetails' or app.request.attributes.get('_route') == 'courseunitDetails' %} active {% endif %}" href="{{ app.request.baseUrl }}/qualifications"><i class="material-icons">school</i><span>Qualification</span></a></li>
                                {% endif %}
                                {% if ('ROLE_APPLICANT' not in roles)  %}
                                    <li>
                                        <svg class="hide" id="portfolioCount" height="25" width="25" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" x="0px" y="0px"
                                             viewBox="0 0 25 25" style="enable-background:new 0 0 25 25;" xml:space="preserve">
                                            <circle style="fill:#DF1F26;" cx="12.5" cy="12.5" r="12.5"/>
                                            <text text-anchor="middle" x="9" y="0" transform="matrix(1 0 0 1 3.266 16.022)" style="fill:#FFFFFF;" class="portfolio-current"></text>
                                        </svg>
                                        <a class="{% if app.request.attributes.get('_route') == 'applicants' %} active {% endif %}" href="{{ app.request.baseUrl }}/applicants" {% if ('ROLE_FACILITATOR' in roles)  %} target="_blank" {% endif %}><i class="material-icons">account_circle</i><span>portfolios</span></a>
                                    </li>
                                {% endif %}
                                {% if ('ROLE_SUPERADMIN' not in roles) %}  
                                    <li>
                                        <svg class="hide" id="messageCount" height="25" width="25" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" x="0px" y="0px"
                                             viewBox="0 0 25 25" style="enable-background:new 0 0 25 25;" xml:space="preserve">
                                            <circle style="fill:#DF1F26;" cx="12.5" cy="12.5" r="12.5"/>
                                            <text id="unread-count" text-anchor="middle" x="9" y="0" transform="matrix(1 0 0 1 3.266 16.022)" style="fill:#FFFFFF;"></text>
                                        </svg>
                                        <a class="{% if app.request.attributes.get('_route') == 'messages' %} active {% endif %}" href="{{ app.request.baseUrl }}/messages" {% if ('ROLE_FACILITATOR' in roles)  %} target="_blank" {% endif %}><i class="material-icons">message</i><span>Messages</span></a>
                                    </li>
                                {% endif %}
                                {% if ('ROLE_FACILITATOR' in roles)  %}
                                    <li>
                                        <svg class="hide" id="evidenceCount" height="25" width="25" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" x="0px" y="0px"
                                             viewBox="0 0 25 25" style="enable-background:new 0 0 25 25;" xml:space="preserve">
                                            <circle style="fill:#DF1F26;" cx="12.5" cy="12.5" r="12.5"/>
                                            <text text-anchor="middle" x="9" y="0" transform="matrix(1 0 0 1 3.266 16.022)" style="fill:#FFFFFF;" class="evidence-current"></text>
                                        </svg>
                                        <a class="{% if app.request.attributes.get('_route') == 'evidences' %} active {% endif %}" href="{{ app.request.baseUrl }}/evidences" {% if ('ROLE_FACILITATOR' in roles)  %} target="_blank" {% endif %}><i class="material-icons">cloud_upload </i><span> Evidence </span></a>
                                    </li>
                                {% endif %}
                                {% if ('ROLE_APPLICANT' in roles)  %}
                                        <li><a class="{% if app.request.attributes.get('_route') == 'files' %} active {% endif %}" href="{{ app.request.baseUrl }}/files"><i class="material-icons">cloud_upload </i><span> Files </span></a></li>
                                {% endif %}
                                {% if ('ROLE_MANAGER' in roles or 'ROLE_SUPERADMIN' in roles) %}
                                    {% if not is_mobile() %}   
                                        <li><a class="{% if app.request.attributes.get('_route') == 'manageusers' %} active {% endif %}" href="{{ app.request.baseUrl }}/manageusers"><!--i class="material-icons">group</i--><span>Manage Users</span></a></li>
                                    {% endif %}
                                {% endif %}
                                {% if(not is_mobile()) %}
                                    {% if ( muser and (('ROLE_FACILITATOR' in roles) or ('ROLE_ASSESSOR' in roles) or ('ROLE_RTO' in roles))) %} 
                                        <li class="pull-left gq-assessor-title-download gq-back-user"><a href="{{ app.request.baseUrl }}/userLogin/{{muser}}"><i class="material-icons">cached</i><span class="gq-assessor-title-back-icon">Switch Profile</span></a> </li>
                                    {% elseif ( suser and ('ROLE_MANAGER' in roles) ) %}
                                        <li class="pull-left gq-assessor-title-download gq-back-user"><a href="{{ app.request.baseUrl }}/userLogin/{{suser}}"><i class="material-icons">cached</i><span class="gq-assessor-title-back-icon">Switch Profile</span></a> </li>
                                    {% endif %}
                                {% endif %}
                                {% if ('ROLE_MANAGER' in roles or 'ROLE_SUPERADMIN' in roles) and (is_mobile())  %}
                                    <li class="hamburger-menu"><a href="javascript:void(0);"><i class="material-icons">menu</i></a></li>
                                {%endif%}
                                {%if not is_mobile() %}
                                    <li class="profile">
                                        <a href="#"><img class="profile-img" src="{{ asset('public/images/PROFILE.png') }}" alt="profile" /><i class="material-icons">expand_more</i></a>
                                        <ul class="hide {% if ('ROLE_SUPERADMIN' in roles  and not is_mobile())%}admin-profile{%endif%}">
                                            {% if ('ROLE_SUPERADMIN' in roles  and not is_mobile())  %}
                                                <li><a class="{% if app.request.attributes.get('_route') == 'userslog' %} active {% endif %}" href="{{ app.request.baseUrl }}/userslog"><!--i class="material-icons">book</i--><span>Logs</span></a></li>
                                                <li><a class="{% if app.request.attributes.get('_route') == 'reports' %} active {% endif %}" href="{{ app.request.baseUrl }}/reports"><!--i class="material-icons">assignment</i--><span>Reports</span></a></li>
                                            {% endif %}
                                            {% if ('ROLE_APPLICANT' not in roles)  %}<li><a data-toggle="modal" data-target="#profile2" href="javascript:void(0)">Profile</a></li>
                                            {% else %}<li><a href="{{ app.request.baseUrl }}/userprofile">Profile</a></li>
                                            {% endif %}
                                            <li><a href="{{ app.request.baseUrl }}/logout">Log out</a></li>
                                        </ul>
                                    </li>
                                {%endif%}
                            </ul>
                            {% if ('ROLE_MANAGER' in roles or 'ROLE_SUPERADMIN' in roles) and (is_mobile())  %}
                                <nav class="manager-nav hide">
                                    <ul>
                                        {% if ('ROLE_MANAGER' in roles or 'ROLE_SUPERADMIN' in roles ) %}<li><a href="{{ app.request.baseUrl }}/manageusers"><!--i class="material-icons">group</i--><span>Manage Users</span></a></li>{%endif%}
                                        {% if ('ROLE_SUPERADMIN' in roles) %}<li><a href="{{ app.request.baseUrl }}/userslog"><span>Logs</span></a></li><li><a href="{{ app.request.baseUrl }}/reports"><span>Reports</span></a></li>{%endif%}
                                    </ul>
                                </nav>
                            {%endif%}
                        </nav>
                    </section>
                </header>    
            {% endblock %}
            <!-- Dashboard page starts here -->
            {% block body %}{% endblock %}
            <div class="mobi-profile hidden-sm hidden-md hidden-lg">
                <ul class="clearfix">
                    <li class="profile-tag">
                        {% set userFName = app.security.getToken().getUser().getFirstName()%}
                        {% set userLName = app.security.getToken().getUser().getLastName()%}
                        {% set userName = userFName~' '~userLName %}
                        {% set userImage = app.security.getToken().getUser().getUserImage()%}
                        {% if ('ROLE_APPLICANT' in roles)  %}
                           <a href="{{ app.request.baseUrl }}/userprofile"><img class="profile-img" src="{{ asset('public/images/PROFILE.png') }}" alt="profile"><label class="profile-tag">{{userName}}</label></a>
                       {% else %}
                           {% set userId = app.security.getToken().getUser().getId() %}
                           {% set userImg = user_service.userImage(userImage,userId) %}
                           <a data-toggle="modal" data-target="#profile2" href="javascript:void(0)"><img class="profile-img" src="{{userImg}}" alt="profile"><label class="profile-tag">{{userName}}</label></a>
                       {% endif %}
                    </li>
                    <li class="sign-out"><a href="{{ app.request.baseUrl }}/logout">Logout</a></li>
                    {% if(is_mobile()) %}
                        {% set muser = app.session.get('muser') %}
                        {% set suser = app.session.get('suser') %}
                         {% if ( muser and (('ROLE_FACILITATOR' in roles) or ('ROLE_ASSESSOR' in roles) or ('ROLE_RTO' in roles))) %} 
                            <li class="gq-back-user"><a href="{{ app.request.baseUrl }}/userLogin/{{muser}}"><i class="material-icons">cached</i><span class="gq-assessor-title-back-icon"></span></a> </li>
                        {% elseif ( suser and ('ROLE_MANAGER' in roles) ) %}
                            <li class="gq-back-user"><a href="{{ app.request.baseUrl }}/userLogin/{{suser}}"><i class="material-icons">cached</i><span class="gq-assessor-title-back-icon"></span></a> </li>
                        {% endif %} 
                    {% endif %} 

                </ul>
            </div>
            <!-- Dashboard page ends here -->
    </main>
    {% block outofbody %}{% endblock %}
    <!-- Profile Modal Starts -->
    {% if ('ROLE_APPLICANT' not in roles)  %}
        {% if is_mobile() and  not is_tablet() %}   
            {% include 'GqAusUserBundle:User/mobile:profile_popup.html.twig' %}
        {% elseif is_tablet() %}
            {% include 'GqAusUserBundle:User:profile_popup.html.twig' %}
        {% else %}                
            {% include 'GqAusUserBundle:User:profile_popup.html.twig' %}
        {% endif %}
    {% endif %}
    <!-- Profile Modal Ends -->
    <!-- jQuery first, then Bootstrap JS. -->     
    <script src="{{ asset('public/js/jquery.min.js') }}"></script>
    <script src="{{ asset('public/js/jquery-1.10.2.js') }}"></script>
    <script src="{{ asset('public/js/jquery-ui.js')}}"></script>   
    <script src="{{ asset('public/js/jquery.fancybox.js?v=2.1.5') }}"></script>
    <script src="{{ asset('public/js/jquery-ui-timepicker-addon.js')}}"></script>
    <script src="{{ asset('public/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('public/js/jquery.form.js') }}"></script>
    <script src="{{ asset('public/js/script.js') }}"></script>
    <script src="{{ asset('public/js/jquery.dataTables.min.js') }}"></script>
    <script src="{{ asset('public/js/custom-jquery.js') }}"></script>   
    {% block javascripts %}{% endblock %}
    <script>
        onloadCount();
        onloadPortfolioCount();
        onloadEvidenceCount();
    </script>
    <script type="text/javascript">
        $(document).ready(function() {
            $('.fancybox').fancybox();
        });
    </script>
  </body>
</html>