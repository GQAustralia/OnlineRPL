<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block title %}{% endblock %}</title>
    <meta name="description" content="Online RPL">
    <meta name="author" content="GQA App Dev">
    <meta name="copyright" content="Get Qualified Australia">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

    <!-- core -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700">
    <link rel="stylesheet"
          href="{{ asset('public/newdesign/fonts/material-design-iconic-font/css/material-design-iconic-font.min.css') }}"/>
    <link rel="stylesheet" href="{{ asset('public/newdesign/css/bootstrap/bootstrap.min.css') }}"/>
    <link rel="stylesheet" href="{{ asset('public/newdesign/css/global.min.css') }}"/>

    <link rel="stylesheet" href="{{ asset('public/newdesign/css/modules/login/login.css') }}"/>

    {% block stylesheets %}
    {% endblock %}

    <script src="{{ asset('public/newdesign/js/plugins/isMobile.min.js') }}"></script>
    <script src="{{ asset('public/newdesign/js/modernizr-3.3.1.gqa-build.121516.min.js') }}"></script>
</head>

<body class="bg-login">

{% block content %}{% endblock %}

<footer class="footer-login" id="footerLogin">
    <section class="footer-content">
        <img src="{{ asset('public/newdesign/img/gqa-logo-full-white-type.svg') }}"
             alt="Get Qualified Australia"
             class="footer-logo img-responsive">
        <ul class="list-inline small text-right" style="margin-top: 10px;">
            <li><a href="https://www.gqaustralia.edu.au/" target="_blank">GQA Home</a></li>
            <li><a href="tel:">Contact Us</a></li>
            <li style="display: block; margin-top: 5px;">Copyright &copy; <span id="currentYear">----</span>
                GQA. All Rights Reserved
            </li>
        </ul>
    </section>
</footer>

    <!-- Core -->
    <script src="{{ asset('public/newdesign/js/jquery-1.11.2.min.js') }}"></script>
    <script src="{{ asset('public/newdesign/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('public/newdesign/js/gqa-utilities.js') }}"></script>
    <script src="{{ asset('public/newdesign/js/velocity.min.js') }}"></script>
    <script src="{{ asset('public/newdesign/js/velocity.ui.min.js') }}"></script>

    <script src="{{ asset('public/newdesign/js/plugins/polyfills.min.js') }}"></script>

    {% block javascripts %}
    {% endblock %}
</body>
</html>