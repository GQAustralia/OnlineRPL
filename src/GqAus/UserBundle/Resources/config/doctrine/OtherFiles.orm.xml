<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="GqAus\UserBundle\Entity\OtherFiles" table="other_files">
  
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="name" type="string" column="name" nullable="false"/>
        <field name="path" type="string" column="path" nullable="false"/>
        <field name="type" type="string" column="type" nullable="false"/>
        <field name="size" type="integer" column="size" nullable="false"/>
		<field name="isdeleted" type="integer" column="isdeleted" nullable="true"/>
        <many-to-one field="assessor" target-entity="GqAus\UserBundle\Entity\User" inversed-by="otherfiles">
            <join-columns>
                <join-column name="user_id" referenced-column-name="id"/>
            </join-columns>
        </many-to-one>
        <field name="created" type="datetime" column="created" nullable="false"/>
    </entity>
</doctrine-mapping>