<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="GqAus\UserBundle\Entity\CourseStatus" table="coursestatus">
  
        <id name="id" type="string" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="unitCode" type="string" column="unit_code" nullable="false"/>
        <field name="createdOn" type="string" column="created_on" nullable="false"/>
        <field name="facilitatorstatus" type="integer" column="facilitatorstatus" nullable="false"/>
        <field name="assessorstatus" type="integer" column="assessorstatus" nullable="false"/>
        <field name="rtostatus" type="integer" column="rtostatus" nullable="false"/>        
        <many-to-one field="course" target-entity="GqAus\UserBundle\Entity\UserCourses">
            <join-column name="user_course_id" referenced-column-name="id" />
        </many-to-one>
    </entity>
</doctrine-mapping>