<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="GqAus\UserBundle\Entity\Schooling" table="schooling">
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="highcompschlevel" type="string" column="highest_completed_school_level" nullable="false"/>   
        <field name="whichyear" type="string" column="which_year" nullable="false"/>   
        <field name="secschlevel" type="string" column="secondary_school_level" nullable="false"/>   
        <one-to-one field="user" target-entity="GqAus\UserBundle\Entity\User">
           <join-column name="user_id" referenced-column-name="id" />
        </one-to-one>
    </entity>
</doctrine-mapping>