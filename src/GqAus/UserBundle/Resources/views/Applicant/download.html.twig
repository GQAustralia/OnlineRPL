<page backtop="40mm" style="padding:0;color:#5e5e5e;font-size:12px;">
        <page_header>
              <table class="page_header">
                    <tr>
                        <td style="width: 100%; text-align: left">
                            <img src="{{ projectPath }}web/public/images/logo.png" alt="GQ Australia" width="250" />
                        </td>
                    </tr>
                </table>
        </page_header>   
          <div>
            <table cellpadding="4" cellspacing="0" border="0" width="100%">
                <tr><td style="font-size:18px;">Personal Details</td></tr>
                <tr><td height="5"></td></tr>

                <tr><td style="width:96%;">
                        {% if user %}
                        <table cellpadding="0" cellspacing="0" border="0" width="100%">
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px;font-weight:bold;width:20%;">First Name</td>
                                <td style="border:1px solid #e7ecee;padding:10px; border-left:none;width:80%;">{{user.firstname}}</td>
                                
                            </tr>
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px; border-top:none;font-weight:bold;">Last Name</td>
                                <td style="border-bottom:1px solid #e7ecee;border-right:1px solid #e7ecee;padding:10px;">{{user.lastname}}</td>
                                
                            </tr>
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px; border-top:none;font-weight:bold;">Email</td>
                                <td style="border-bottom:1px solid #e7ecee;border-right:1px solid #e7ecee;padding:10px;">{{user.email}}</td>
                                
                            </tr>
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px; border-top:none;font-weight:bold;">Gender</td>
                                <td style="border-bottom:1px solid #e7ecee;border-right:1px solid #e7ecee;padding:10px;">{{user.gender}}</td>
                                
                            </tr>
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px; border-top:none;font-weight:bold;">Phone</td>
                                <td style="border-bottom:1px solid #e7ecee;border-right:1px solid #e7ecee;padding:10px;">{{user.phone}}</td>
                                
                            </tr>
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px; border-top:none;font-weight:bold;">Date Of Birth</td>
                                <td style="border-bottom:1px solid #e7ecee;border-right:1px solid #e7ecee;padding:10px;">{{user.dateOfBirth}}</td>
                                
                            </tr>
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px; border-top:none;font-weight:bold;">USI</td>
                                <td style="border-bottom:1px solid #e7ecee;border-right:1px solid #e7ecee;padding:10px;">{{user.universalStudentIdentifier}}</td>
                                
                            </tr>
                        </table>
                        {% endif %}
                    </td>
                </tr>
                <tr><td height="20"></td></tr>

       
            </table>
            <table cellpadding="4" cellspacing="0" border="0" width="100%">
                <tr><td style="font-size:18px;">User Address</td></tr>
                <tr><td height="5"></td></tr>

                <tr><td style="width:96%;">
                        {% if user %}
                        <table cellpadding="0" cellspacing="0" border="0" width="100%">
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px;font-weight:bold;width:20%;">Street Number</td>
                                <td style="border:1px solid #e7ecee;padding:10px; border-left:none;width:80%;">{{user.address.address}}</td>
                                
                            </tr>
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px; border-top:none;font-weight:bold;">Street Name</td>
                                <td style="border-bottom:1px solid #e7ecee;border-right:1px solid #e7ecee;padding:10px;">{{user.address.area}}</td>
                                
                            </tr>
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px; border-top:none;font-weight:bold;">Suburb</td>
                                <td style="border-bottom:1px solid #e7ecee;border-right:1px solid #e7ecee;padding:10px;">{{user.address.suburb}}</td>
                                
                            </tr>
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px; border-top:none;font-weight:bold;">City</td>
                                <td style="border-bottom:1px solid #e7ecee;border-right:1px solid #e7ecee;padding:10px;">{{user.address.city}}</td>
                                
                            </tr>
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px; border-top:none;font-weight:bold;">State</td>
                                <td style="border-bottom:1px solid #e7ecee;border-right:1px solid #e7ecee;padding:10px;">{{user.address.state}}</td>
                                
                            </tr>
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px; border-top:none;font-weight:bold;">Country</td>
                                <td style="border-bottom:1px solid #e7ecee;border-right:1px solid #e7ecee;padding:10px;">{{user.address.country}}</td>
                                
                            </tr>
                            <tr>
                                <td style="border:1px solid #e7ecee;padding:10px; border-top:none;font-weight:bold;">Postcode</td>
                                <td style="border-bottom:1px solid #e7ecee;border-right:1px solid #e7ecee;padding:10px;">{{user.address.pincode}}</td>
                                
                            </tr>
                        </table>
                        {% endif %}
                    </td>
                </tr>
                <tr><td height="20"></td></tr>

       
            </table>
          </div>
         </page>
         <page backtop="40mm" style="padding:0;color:#5e5e5e;font-size:12px;">
        <page_header>
              <table class="page_header">
                    <tr>
                        <td style="width: 100%; text-align: left">
                            <img src="{{ projectPath }}web/public/images/logo.png" alt="GQ Australia" width="250" />
                        </td>
                    </tr>
                </table>
        </page_header>  
          <div>     
            <table cellpadding="4" cellspacing="0" border="0" width="100%">
                <tr><td style="font-size:18px;">User ID Files</td></tr>
                <tr><td height="5"></td></tr>
            
                    {% if userIdFiles %}
                    {% for item in userIdFiles %}
                        <tr>
                            <td style="border:1px solid #e7ecee;padding:10px;width:96%;"><a href="{{ base_url }}viewIdFile/{{ item.id }}"  style="color:#5e5e5e;text-decoration:none;" target="_new">{{ item.type.type }}</a></td>
                        </tr>
                    {% endfor %}
                    {% else %}
                    <tr>    
                    <td><center id="idfiles_no_files">No Id files found</center></td>
                    </tr>
                   {% endif %}                    
                <tr><td height="20"></td></tr>

       
            </table>
           </div>
         </page>
         <page backtop="40mm" style="padding:0;color:#5e5e5e;font-size:12px;">
        <page_header>
              <table class="page_header">
                    <tr>
                        <td style="width: 100%; text-align: left">
                            <img src="{{ projectPath }}web/public/images/logo.png" alt="GQ Australia" width="250" />
                        </td>
                    </tr>
                </table>
        </page_header>  
           <div>     
            <table cellpadding="4" cellspacing="0" border="0" width="100%">
                <tr><td style="font-size:18px;">Applicant Details</td></tr>
                <tr><td height="20"></td></tr>

                <tr><td style="font-size:16px; background:#f0f0f0;width:96%;padding:6px;">{{courseInfo.id}} {{courseInfo.name}}</td></tr>	
                <tr><td style="width:96%;">
                        <table cellpadding="0" cellspacing="0" border="0" width="100%">
                            <tr>
                                <td style="width:35%;">
                                    <table cellpadding="4" cellspacing="0" border="0">
                                        <tr><td></td><td style="font-weight:bold;">{{userName}}</td></tr>
                                    </table>
                                </td>
                                <td style="width:30%;">
                                    <table cellpadding="4" cellspacing="0" border="0">
                                        <tr><td>ID Points : </td><td style="font-weight:bold;">{{currentIdPoints}}</td></tr>
                                        <tr><td>Profile : </td><td style="font-weight:bold;">{{profileCompleteness}}</td></tr>
                                    </table>
                                </td>
                                <td style="width:35%;">
                                        {% set userRole = app.security.getToken().getUser().getRoles() %}
                                    <table cellpadding="4" cellspacing="0" border="0">
                                                    {% if ('ROLE_ASSESSOR' in userRole) %}
                                        <tr><td>Rto : </td><td style="font-weight:bold;">{{rtoName}}</td></tr>
                                                    {% endif %}
                                                    {% if ('ROLE_RTO' in userRole) %}
                                        <tr><td>Assessor : </td><td style="font-weight:bold;">{{assessorName}}</td></tr>
                                                    {% endif %}
                                        <tr><td>Facilitator : </td><td style="font-weight:bold;">{{facilitatorName}}</td></tr>
                                    </table>
                                </td>
                            </tr>
                        </table>
                    </td></tr>
                <tr><td height="20"></td></tr>

        {% if courseInfo %}
        {% if courseInfo.Units %}
            {% if courseInfo.Units.Unit %}

                <tr><td style="font-size:16px; background:#f0f0f0;width:96%;padding:6px;">Core Units</td></tr>	
                {% for key, units in courseInfo.Units.Unit %}
                    {% if units.type == 'core' %}
                        <tr><td style="font-size:14px; background:#f0f0f0;width:96%;padding:6px;">{{units.id}}&nbsp;&nbsp;{% spaceless %}{{units.name|trim}}{% endspaceless %}</td></tr> 
                        {% if units.unitEvidences %} 
                        {% for unitEvidence in units.unitEvidences%}                        
                        <tr><td style="padding:10px;border:1px solid #e7ecee;width:96%;">
                             {% if unitEvidence.type != 'text' %}   
                                 <a href="{{ base_url }}viewEvidence/{{ unitEvidence.id }}"  style="color:#464646;text-decoration:none;" target="_blank">{{ unitEvidence.pathName }}</a>
                             {% else %}
                                 <span>{{ unitEvidence.content }}</span>
                             {% endif %}
                         </td></tr>

                        {% endfor %}
                        {% else %}
                        <tr><td style="padding:10px;border:1px solid #e7ecee;width:96%;">No Evidences found</td></tr>
                        {% endif %}
                    {% endif %}
                  {% endfor %}
                 <tr><td height="20"></td></tr>    
                <tr><td style="font-size:16px; background:#f0f0f0;width:96%;padding:6px;">Elective Units</td></tr>	
                {% set flagElective = '0' %}
                {% for key, eUnits in courseInfo.Units.Unit %}
                   {% if eUnits.type == 'elective' %}
                       {% set unitId = eUnits.id %}
                       {% if unitId|trim in electiveUnits|keys %} 
                       {% set flagElective = '1' %}
                       <tr><td style="font-size:14px; background:#f0f0f0;width:96%;padding:6px;">{{eUnits.id}}&nbsp;&nbsp;{% spaceless %}{{eUnits.name|trim}}{% endspaceless %}</td></tr> 
                       {% if eUnits.unitEvidences %} 
                       {% for unitEvidence in eUnits.unitEvidences%}
                       <tr><td style="padding:10px;border:1px solid #e7ecee;width:96%;">
                           {% if unitEvidence.type != 'text' %} 
                               <a href="{{ base_url }}viewEvidence/{{ unitEvidence.id }}"  style="color:#464646;text-decoration:none;" target="_blank">{{ unitEvidence.pathName }}</a>
                           {% else %}
                                <span>{{ unitEvidence.content }}</span>
                           {% endif %}    
                       </td></tr>
                       {% endfor %}
                       {% else %}
                       <tr><td style="padding:10px;border:1px solid #e7ecee;width:96%;">No Evidences found</td></tr>
                       {% endif %}
                   {% endif %}
                {% endif %}
                {% endfor %}
                {% if flagElective == '0' %}
                        <tr><td style="padding:10px;border:1px solid #e7ecee;width:96%;">No Elective Units found</td></tr>
                {% endif %}       
                 

        {% endif %}
            {% endif %}
         {% endif %}
            </table>
          </div>
         </page> 