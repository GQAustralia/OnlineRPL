{% extends 'base.html.twig' %}
{% block title %}{% if userName %}{{userName}}{% endif %}{% endblock %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('public/css//bootstrap-datetimepicker.css')}}">
    <link rel="stylesheet" href="{{ asset('public/css/pages/all/qualififation.css') }}">
    <link rel="stylesheet" href="{{ asset('public/css/pages/all/profile.css')}}">
{% endblock %}
{% block navmenu %}
    <header>
        <section class="container">
            <div class="info-header">
                <div class="portfolio">
                    <strong class="title">{{userName}}</strong>
                    <a href="#"><i class="material-icons clear">clear</i></a>
                </div>
                <div class="unit-info hide">
                    <a href="#"><i class="material-icons btn-back">arrow_back</i></a>
                    <strong class="title">Unit info</strong>
                </div>            
            </div>
            <nav>
                <ul class="nav">
                    <li><a {% if app.request.attributes.get('_route') == 'applicantDetails'%} class="active" {% endif %} href="{{ app.request.baseUrl }}/applicantDetails/{{courseInfo.id}}/{{userId}}"><i class="material-icons">school</i><span>Qualification</span></a></li>
                    <li><a href="#"><i class="material-icons">cloud_upload</i><span>Files</span></a></li>
                    <li><a {% if app.request.attributes.get('_route') == 'candidateProfile'%} class="active" {% endif %} href="{{ app.request.baseUrl }}/candidateProfile/{{userId}}"><i class="material-icons">account_circle</i><span>Profile</span></a>
                </li>
                <li class="profile">
                    <a href="#"><img class="profile-img" src="{{ asset('public/images/PROFILE.png')}}" alt="profile" /><i class="material-icons">expand_more</i></a>
                    <ul class="hide">
                        <li><a href="#" data-toggle="modal" data-target="#currProfile">Profile</a></li>
                        <li><a href="/logout">Log out</a></li>
                    </ul>
                </li>
            </ul>          
            </nav>
        </section>
    </header>
{% endblock %}    
{% block body %}
    {% set userRole = app.security.getToken().getUser().getRoles() %}
    {% set firstName = app.security.getToken().getUser().getFirstName() | capitalize %}
    {% set lastName = app.security.getToken().getUser().getLastName() | capitalize %}
    {% set email = app.security.getToken().getUser().getEmail() %}
    {% set mobile = app.security.getToken().getUser().getPhone() %}
    
    {% set facProName = ('ROLE_FACILITATOR' in userRole) ? 'You ' : facilitatorName %}
    {% set facVocName = ('ROLE_FACILITATOR' in userRole) ? 'are' : 'is' %}
    {% set assProName = ('ROLE_ASSESSOR' in userRole) ? 'You ' : assessorName %}
    {% set assVocName = ('ROLE_ASSESSOR' in userRole) ? 'are' : 'is' %}
    {% set rtoProName = ('ROLE_FACILITATOR' in userRole) ? 'You ' : rtoName %}
    {% set rtoVocName = ('ROLE_FACILITATOR' in userRole) ? 'are' : 'is' %}
    <!-- Small Screens HTML -->
    <div class="hidden-lg body_section">
        <div class="presentation-section">
        <div class="container">
            <div class="row">
                <div class="col-sm-12 col-lg-11">
                    <p><span>{% if courseInfo.name %} {{courseInfo.name}} {% endif %}</span><span>{% if courseInfo.id %} {{courseInfo.id}} {% endif %}</span></p>
                </div>
            </div>
        </div>
    </div>
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-sm-12 nopadding col-md-6 col-lg-4">
                    <div class="progres_bar_section">
                        <div class="progres-content">
                            <p class="title">Evidence Submitted - {{evidenceCompleteness}} Complete </p>
                            <div class="progress">
                                <div class="progress-bar" role="progressbar" aria-valuenow="{{evidenceCompleteness}}" aria-valuemin="0" aria-valuemax="100" style="width:{{evidenceCompleteness}}">
                                  <span class="sr-only">{{evidenceCompleteness}} Complete</span>
                                </div>
                            </div>    
                        </div>
                        {% set remaining = user_service.getDaysRemainingFromRole(userId,courseInfo.id, userRole[0]) %}
                        {% set output = remaining|split('&&') %}
                        <div class="progres-content">
                            <p class="title">Days remaining - {% if output[0] >0 %} {{ output[0] }} {%else%} 0 {% endif %}days</p>
                            <div class="progress">
                                <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width:{%if (output[1] > 0) %}{{output[1]}}{%else%}0{%endif%}%">
                                  <span class="sr-only">{%if (output[1] > 0) %}{{output[1]}}{%else%}0{%endif%}% Complete</span>
                                </div>
                            </div>                        
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-6 col-lg-7">
                    <div class="panel-group" id="accordion1">
                        <div class="panel panel-default">
                            <div class="panel-heading white clearfix">
                                <h4 class="panel-title">
                                    <span>Submitted to RTO</span>
                                    <a href="#nested-collapseSTR" data-parent="#nested" data-toggle="collapse" class="btn-accordion">
                                        <i class="material-icons down">keyboard_arrow_down</i>
                                    </a>                                
                                </h4>
                            </div>
                        </div>            
            {% set i = 1 %}
                    {% if courseInfo %}
                        {% if courseInfo.Units %}
                            {% if courseInfo.Units.Unit %}
                        <div class="panel panel-default">    
                            <div class="panel-heading clearfix">
                                <h4 class="panel-title">
                                    <span>{{coreUnitsCount}} CORE UNITS <strong>Competent</strong></span>
                                    <a class="btn-accordion collapsed" data-toggle="collapse" data-parent="#accordion" href="#xs-collapseTwo">
                                        <i class="material-icons down">keyboard_arrow_down</i>
                                    </a>                                
                                </h4>
                            </div>
                            <div id="xs-collapseTwo" class="panel-collapse collapse">
                                    <div class="panel-body">
                                        <div class="panel-group" id="nested">
                                            {% set flagCore = '0' %}
                                        {% set unitsData = courseInfo.Units.Unit %}
                                        {% if courseInfo.Units.Unit.id is defined %}
                                            {% set unitsData = {0: courseInfo.Units.Unit} %}
                                        {% endif %}
                                        {% for key, units in unitsData %}
                                            {% if units.type == 'core' %}
                                        {% set flagCore = '1' %}
                                                <div class="panel panel-default">
                                                        <div class="panel-heading">
                                                                <div class="panel-title">
                                                                        <div class="panle_body_content">
                                                                                <p> <span class="tag-id">{{units.id}}</span> </p>
                                                                                <p>{{units.name}}</p>
                                                                        </div>
                                                                        <a data-toggle="collapse" data-parent="#nested" href="#xs-nested-collapse{{units.id}}">
                                                                                <i class="material-icons keyboard_arrow_right">keyboard_arrow_right</i>
                                                                        </a>
                                                                </div>
                                                        </div>
                                                        <div id="xs-nested-collapse{{units.id}}" class="panel-collapse collapse">
                                                            <div class="panel-body">{% if units.details %} {{units.details|raw}}{% endif %} </div>
                                                        </div>
                                                </div>
                                            {% set i = i+1 %}
                                        {% endif %}
                                        {% endfor %}
                                        {% if flagCore == '0' %}
                                                <div class="gq-acc-row-title load-icon-tr">No Core Units found</div>
                                        {% endif %}
                                        </div>
                                    </div>
                            </div>
                        </div>
                        <div class="panel panel-default">
                            <div class="panel-heading clearfix">
                                <h4 class="panel-title">
                                    <span>{{electiveUnitsCount}} ELECTIVE UNITS - <strong>Competent</strong></span>
                                    <a class="btn-accordion collapsed" data-toggle="collapse" data-parent="#accordion" href="#xs-collapseThree">
                                        <i class="material-icons down">keyboard_arrow_down</i>
                                    </a>                                
                                </h4>
                            </div>
                            <div id="xs-collapseThree" class="panel-collapse collapse">
                                <div class="panel-body">
                                    <div class="panel-group" id="nested">
                                {% if courseInfo.Units %}
                                        {% if courseInfo.Units.Unit %}
                                            {% set flagElective = '0' %}
                                            {% set unitsData = courseInfo.Units.Unit %}
                                            {% if courseInfo.Units.Unit.id is defined %}
                                            {% set unitsData = {0: courseInfo.Units.Unit} %}
                                            {% endif %}
                                            {% for key, units in unitsData %}
                                                {% if units.type == 'elective' %}
                                                    {% set unitId = units.id %}
                                                    {% if unitId|trim in electiveUnits|keys %} 
                                                    {% set flagElective = '1' %}
                                        <div class="panel panel-default">
                                            <div class="panel-heading">
                                                <div class="panel-title">
                                                    <div class="panle_body_content">
                                                        <p> <span class="tag">Competent</span> </p>
                                                        <p> <span class="tag-id">{{units.id}}</span> </p>
                                                        <p>{{units.name}} </p>
                                                    </div>
                                                    <a data-toggle="collapse" data-parent="#nested" href="#xs-nested-collapse{{units.id}}">
                                                        <i class="material-icons keyboard_arrow_right">keyboard_arrow_right</i>
                                                    </a>
                                                </div>
                                            </div>
                                            <div id="xs-nested-collapse{{units.id}}" class="panel-collapse collapse">
                                                <div class="panel-body">{% if units.details %} {{units.details|raw}}{% endif %}</div>
                                            </div>
                                        </div>
                                                {% set i = i+1 %}
                                                {% else %}
                                                {% endif %}
                                                {% endif %}
                                            {% endfor %}
                                            {% if flagElective == '0' %}
                                                    <div class="gq-acc-row-title load-icon-tr">No Elective Units found</div>
                                            {% endif %}
                                         {% endif %}
                                    {% endif %}
                                    </div>
                                </div>
                            </div>
                        </div>
                        {% endif%}
                    {% endif%}
                {% endif%}
                        <div class="gqa-btn-group">
                            <div class="btn">
                                <div class="btn-msg">
                                    <a href="#">Message</a>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                </div>
                <div class="col-xs-12 col-sm-12 nopadding col-md-6 col-lg-4">
                    <div class="review">
                        <div class="review_content">
                            <p><strong class="text-uppercase">Rto</strong> - {{rtoName}}</p>
                        </div>
                        <div class="review_profile-pic"><span></span></div>
                    </div>
                    <div class="review">
                        <div class="review_content">
                            <p><strong class="text-uppercase">{{facilitatorName}} is </strong> {{userName}}'s CSR for this course</p>
                        </div>
                        <div class="review_profile-pic">
                            <span><img class="profile-img" src="{% if facilitatorImage %} {{ facilitatorImage }} {% endif %}" /></span>
                        </div>
                    </div>
                    <div class="review">
                        <div class="review_content"><p><strong class="text-uppercase">{{assessorName}} is </strong> {{userName}}'s assessor for this cource</p></div>
                        <div class="review_profile-pic"><span><img class="profile-img" src="{% if assessorImage %} {{ assessorImage }} {% endif %}" /></span></div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-12 nopadding col-md-6 col-lg-4">
                    {% if ('ROLE_RTO' in userRole) %}
                        {% if (rtoUnitStatus == '1') %}
                            <div class="form-group form-group-lg">
                            {% if ( courseStatus == '15') %}
                                <button type="submit" courseCode = "{{courseInfo.id}}" applicantId="{{userId}}" id="approve-for-certification" class="btn btn-green approve-for-certification-btn">Approved</button>
                             {% elseif ( courseStatus == '0' or courseStatus == '16') %}
                                <button type="button" class="btn btn-danger">Not yet approved</button>                                 
                            {% endif %}
                            </div>
                        {% endif %}
                    {% endif %}
                </div>
            </div>
        </div>    
    </div>
    <!-- Small Screens HTML End -->
    
    <!-- Large Screens HTML -->
    <div class="hidden-xs body_section">
        <div class="presentation-section">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12 col-lg-11">
                        <p>
                            <span>{% if courseInfo.name %} {{courseInfo.name}} {% endif %}</span>
                            <span>{% if courseInfo.id %} {{courseInfo.id}} {% endif %}</span>
                        </p>
                    </div>
                </div>
            </div>
        </div>
        <div class="status-message" id="status-message" style="display:none;"></div>
        <div class="container">
            <div class="row">
                {% if ('ROLE_ASSESSOR' in userRole) %}
                <div class="col-lg-11">
                    <div class="gqa-btn-group">
                        <div class="btn border-dark">
                            <i class="material-icons">file_download</i>
                            <a download="" href="#">Download Assessment Kit for this qualification</a>
                        </div>                    
                    </div>
                </div>
                {% endif %}
                <div class="col-xs-12 col-sm-12 nopadding col-md-6 col-lg-4">
                    <div class="progres_bar_section">
                        <div class="progres-content">
                            <p class="title">Evidence Submitted - {{evidenceCompleteness}} Complete </p>
                            <div class="progress">
                                <div class="progress-bar" role="progressbar" aria-valuenow="{{evidenceCompleteness}}" aria-valuemin="0" aria-valuemax="100" style="width:{{evidenceCompleteness}}">
                                  <span class="sr-only">{{evidenceCompleteness}} Complete</span>
                                </div>
                            </div>                        
                        </div>
                        {% set remaining = user_service.getDaysRemainingFromRole(userId,courseInfo.id, userRole[0]) %}
                        {% set output = remaining|split('&&') %}
                        <div class="progres-content">
                            <p class="title">Days remaining :{% if output[0] >0 %} {{ output[0] }} {%else%} 0 {% endif %}days</p>
                            <div class="progress">
                                <div class="progress-bar" role="progressbar" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100" style="width:{%if (output[1] > 0) %}{{output[1]}}{%else%}0{%endif%}%">
                                  <span class="sr-only">{%if (output[1] > 0) %}{{output[1]}}{%else%}0{%endif%}% Complete</span>
                                </div>
                            </div>                        
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-lg-12">
                            <div class="review-container">
                                <div class="review">
                                    <div class="review_content">
                                        <p><strong class="text-uppercase">Rto</strong> - {{rtoName}}</p>
                                    </div>
                                    <div class="review_profile-pic"><span></span></div>
                                </div>
                                <div class="review">
                                    <div class="review_content">
                                        <p><strong class="text-uppercase">{{facProName}}</strong> {{facVocName}} {{userName}}'s CSR for this course</p>
                                    </div>
                                    <div class="review_profile-pic"><span><img class="profile-img" src="{% if facilitatorImage %} {{ facilitatorImage }} {% endif %}" /></span></div>
                                </div>
                                <div class="review">
                                    <div class="review_content">
                                        <p><strong class="text-uppercase">{{assProName}}</strong> {{assVocName}} {{userName}}'s assessor for this cource</p>
                                    </div>
                                    <div class="review_profile-pic"><span><img class="profile-img" src="{% if assessorImage %} {{ assessorImage }} {% endif %}" /></span></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    {% if ('ROLE_FACILITATOR' in userRole) or ('ROLE_ASSESSOR' in userRole) %}
                        {% if notes|length > 0 or  notesFromCourse|length > 0 %}
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="comments">
                                        <div class="panel-group">
                                            <div class="panel-heading">
                                                <h4 class="panel-title">Notes <span class="count">({{notes|length}})</span></h4>
                                            </div>
                                            {% for note in notes %} 
                                                <div class="panel-content">
                                                    <img src="{% if userImage %} {{ userImage }} {% endif %}" class="profile-img">
                                                    <span><strong>{{note.userName}}</strong></span>
                                                    <span class="date-time">
                                                        <span class="date">{{ note.created|date('m/d/y')}}</span>
                                                        <span class="time">{{ note.created|date('H:i')}}</span>
                                                    </span>
                                                    <p>{{note.note}}</p>
                                                </div>
                                            {% endfor %}
                                            {% for note in notesFromCourse%}
                                                <div class="panel-content">
                                                    <img src="{% if userImage %} {{ userImage }} {% endif %}" class="profile-img">
                                                    <span><strong>{{userName}}</strong></span>
                                                    <span class="date-time">
                                                        <span class="date">{{ note.created|date('m/d/y')}}</span>
                                                        <span class="time">{{ note.created|date('H:i')}}</span>
                                                    </span>
                                                    <p>{% if note.note is defined %}{{note.note}}{%endif%}</p>
                                                </div>
                                            {% endfor %}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        {% endif %}
                    {% endif %}
                    {% if ('ROLE_FACILITATOR' in userRole) %}
                        <div class="gqa-btn-group">
                            <div class="row no-gutter">     
                                 {% if (facilitatorstatus != '1') %}
                                    {% if (courseStatus == '1') %}
                                        <div class="col-md-12 col-lg-12">
                                            <div class="row no-gutter">
                                                <div class="col-md-12 col-lg-12">
                                                    <button type="button" id="submittoassessor"  class="btn btn-danger" data-courseStatus="2">Submit to Assessor</button>                                
                                                </div>
                                            </div>
                                        </div>
                                    {% endif %}
                                {% endif %}
                            </div>                    
                        </div>
                    {% endif %}
                    <div class="gqa-btn-group">
                        <div class="row no-gutter">
                            {% if ('ROLE_RTO' in userRole) or ('ROLE_ASSESSOR' in userRole) %}
                                <div class="col-md-12 col-lg-12">
                                    <div class="btn border-dark">
                                        <i class="material-icons">file_download</i>
                                        <a href ="javascript:void(0)" id="download_profile" userid="{{userId}}" course_code="{{courseInfo.id}}">Download complete Portfolio</a>
                                    </div>
                                </div>
                            {% endif %}
                            {% if ('ROLE_ASSESSOR' in userRole) %}
                                <div class="col-md-12 col-lg-12">
                                    <div class="btn competency-call"><i class="material-icons">call</i><a download="" href="#">Start Competency Call</a></div>                                    
                                </div>
                            {% endif %}
                            {% if ('ROLE_RTO' in userRole) %}
                                {% if (rtoUnitStatus == '1') %}
                                    <div class="col-md-12 col-lg-12">
                                    {% if ( courseStatus == '15') %}
                                        <button type="submit" courseCode = "{{courseInfo.id}}" applicantId="{{userId}}" id="approve-for-certification" class="btn btn-green approve-for-certification-btn">Approved</button>
                                     {% elseif ( courseStatus == '0' or courseStatus == '16') %}
                                        <button type="button" class="btn btn-danger">Not yet approved</button>                                 
                                    {% endif %}
                                    </div>
                                {% endif %}
                            {% endif %}
                        </div>
                    </div>
                </div>
                <div class="col-xs-12 col-sm-6 col-md-6 col-lg-7">
                    <div class="accordion panel-group" id="accordion1">
                        <input type="hidden" name="csCourseCode" id="csCourseCode" readonly="true" value="{{courseInfo.id}}" />
                        <input type="hidden" name="csUserId" id="csUserId" readonly="true" value="{{userId}}" />
                        <div class="panel panel-default">
                            <div class="select-drop-box clearfix">
                                <select name="courseStatus" id="courseStatus" class="selectpicker"> 
                                {% if ('ROLE_ASSESSOR' in userRole and assessorstatus != 1) or ('ROLE_FACILITATOR' in userRole and courseStatus != 0) %}
                                    {% set courseOrder = statusList[courseStatus].order %}
                                                <option value="">Select Status</option>
                                                 {% if (statusList is defined) %}
                                                    {% set roleAcess = 'Factive' %}
                                                    {% if ('ROLE_ASSESSOR' in userRole) %}
                                                        {% set roleAcess = 'Aactive' %}
                                                    {% endif %}
                                                    {% for statusId, statusData in statusList %}
                                                       {% if ('ROLE_FACILITATOR' in userRole or statusData[roleAcess] == 1) %}
                    <option value="{{statusId}}" {% if courseStatus == statusId %} {{ "selected" }} {% else %} {{ "" }} {% endif %} {% if statusData[roleAcess] == 0 %} {{ "disabled" }} {% elseif ((assessorstatus == 1) and (statusData['order'] < courseStatus) and ('ROLE_FACILITATOR' in userRole) ) %} {{ "disabled" }} {% elseif ((rtostatus == 1) and (statusData['order'] < courseStatus) and ('ROLE_FACILITATOR' in userRole) ) %} {{ "disabled" }} {% else %} {{ "" }} {% endif %}>{{statusData['status']}}</option>
                                                       {% endif %}
                                                    {% endfor %}
                                                 {% endif %}
                                    
                                {% else %}
                                         <option>{{ statusList[courseStatus]['status'] }}</option>
                                {% endif %}
                                </select>   
                            </div>
                        </div>
                        
                {% set i = 1 %}
                    {% if courseInfo %}
                        {% if courseInfo.Units %}
                            {% if courseInfo.Units.Unit %}
                              {% set coreUnitCount = user_service.getCourseCountStatusByRoleWise(userId, userRole[0], courseInfo.id, 'core') %} 
                            <div class="panel panel-default">
                                <div class="panel-heading">
                                    {% set coreUnitsRvd =  (coreUnitCount['noOfNotRvdrcrds'] > 0) ? coreUnitCount['noOfNotRvdrcrds']~' to be reviewed' : "Satisfacty" %}
                                    <h4 class="panel-title"><span>{{coreUnitCount['noOfNotRvdrcrds']+coreUnitCount['noOfRvdRcrds']}} CORE UNITS - <strong>{{coreUnitsRvd}}</strong></span>
                                            <a class="btn-accordion collapsed" data-collapse="parent"  data-toggle="collapse" data-parent="#accordion" href="#collapseTwo">
                                                    <i class="material-icons">expand_more</i>
                                            </a>
                                    </h4>
                                </div>
                                <div id="collapseTwo" class="panel-collapse collapse">
                                    <div class="panel-body">
                                        <div class="panel-group" id="nested">
                                            {% set flagCore = '0' %}
                                            {% set unitsData = courseInfo.Units.Unit %}
                                            {% if courseInfo.Units.Unit.id is defined %}
                                                {% set unitsData = {0: courseInfo.Units.Unit} %}
                                            {% endif %}
                                            {% for key, units in unitsData %}
                                                {% if units.type == 'core' %}
                                            {% set flagCore = '1' %}
                                            <div class="panel panel-default">
                                                <div class="panel-heading no-background">
                                                    <div class="panel-title">
                                                        <div class="panle_body_content">
                                                            {% set inloop = 0 %}
                                                            {% set approvalStatus = user_service.getUnitStatusByRoleWise( userId, userRole[0], units.id, courseInfo.id ) %}
                                                            {% if ('ROLE_ASSESSOR' in userRole and approvalStatus =='0') %} 
                                                                {% set approvalStatus = user_service.getUnitStatusByRoleWise( userId, 'ROLE_FACILITATOR', units.id, courseInfo.id ) %}
                                                                {% set inloop = 1 %}
                                                            {% endif %}   
                                                            {% set approvalText = user_service.getStausByStatus(approvalStatus,inloop,userRole[0]) %}
                                                            {% if approvalText|length > 0 %}<p> <span class="tag status in-progress">{{approvalText}} </span> </p>{% endif %}
                                                            <p> <span class="tag-id">{{units.id}}</span> </p>
                                                            <p><a href="/courseunitDetails/{{ courseInfo.id  }}/{{ units.id }}/{{userId}}">{{units.name}}</a></p>
                                                        </div>
                                                        <a href="/courseunitDetails/{{ courseInfo.id  }}/{{ units.id }}/{{userId}}"><i class="material-icons keyboard_arrow_right">keyboard_arrow_right</i></a>
                                                    </div>
                                                </div>
                                            </div>
                                            {% set i = i+1 %}
                                            {% endif %}
                                        {% endfor %}
                                        {% if flagCore == '0' %}
                                                <div class="gq-acc-row-title load-icon-tr">No Core Units found</div>
                                        {% endif %}
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="panel panel-default">
                                <div class="panel-heading clearfix">
                                    {% set electiveUnits = user_service.getCourseCountStatusByRoleWise(userId, userRole[0], courseInfo.id, 'elective') %} 
                                    {% set electiveUnitsRvd =  (electiveUnits['noOfNotRvdrcrds'] > 0) ? electiveUnits['noOfNotRvdrcrds']~' to be reviewed' : "Satisfacty" %}
                                    <h4 class="panel-title"><span>{{electiveUnits['noOfNotRvdrcrds'] + electiveUnits['noOfRvdRcrds']}} ELECTIVE UNITS - <strong>{{electiveUnitsRvd}}</strong></span>
                                        <a class="btn-accordion collapsed" data-collapse="parent" data-toggle="collapse" data-parent="#accordion" href="#collapseThree">
                                                <i class="material-icons">expand_more</i>
                                        </a>
                                    </h4>
                                </div>
                                <div id="collapseThree" class="panel-collapse collapse">
                                    <div class="panel-body">
                                        <div class="panel-group" id="nested">
                                    {% if courseInfo.Units %}
                                        {% if courseInfo.Units.Unit %}
                                            {% set flagElective = '0' %}
                                            {% set unitsData = courseInfo.Units.Unit %}
                                            {% if courseInfo.Units.Unit.id is defined %}
                                            {% set unitsData = {0: courseInfo.Units.Unit} %}
                                            {% endif %}
                                            {% for key, units in unitsData %}
                                                {% if units.type == 'elective' %}
                                                    {% set unitId = units.id %}
                                                    {% if unitId|trim in electiveUnits|keys %} 
                                                    {% set flagElective = '1' %}
                                                    <div class="panel panel-default">
                                                        <div class="panel-heading no-background">
                                                            <div class="panel-title">
                                                                <div class="panle_body_content">
                                                            {% set inloop = 0 %}
                                                            {% set approvalStatus = user_service.getUnitStatusByRoleWise( userId, userRole[0], units.id, courseInfo.id ) %}
                                                            {% if ('ROLE_ASSESSOR' in userRole and approvalStatus =='0') %} 
                                                                {% set approvalStatus = user_service.getUnitStatusByRoleWise( userId, 'ROLE_FACILITATOR', units.id, courseInfo.id ) %}
                                                                {% set inloop = 1 %}
                                                            {% endif %}   
                                                            {% set approvalText = user_service.getStausByStatus(approvalStatus,inloop,userRole[0]) %}
                                                            {% if approvalText|length > 0 %}<p> <span class="tag status in-progress">{{approvalText}} </span> </p>{% endif %}
                                                                    <p> <span class="tag-id">{{units.id}}</span> </p>
                                                                    <p><a href="/courseunitDetails/{{ courseInfo.id  }}/{{ units.id }}/{{userId}}">{{units.name}}</a></p>
                                                                </div>
                                                                <a href="/courseunitDetails/{{ courseInfo.id  }}/{{ units.id }}/{{userId}}"><i class="material-icons keyboard_arrow_right">keyboard_arrow_right</i></a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    {% set i = i+1 %}
                                                    {% else %}
                                                    {% endif %}
                                                {% endif %}
                                            {% endfor %}
                                            {% if flagElective == '0' %}
                                                    <div class="gq-acc-row-title load-icon-tr">No Elective Units found</div>
                                            {% endif %}
                                         {% endif %}
                                    {% endif %}
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {% endif %}
                        {% endif %}
                {% endif %}
                    {% set colsLng = user_service.separateTabsBasedOnroleWise(userRole[0]) %}
                    <div class="gqa-btn-group border-dark no-margin">
                        <div class="row no-gutter">
                            <div class="col-md-{{colsLng}} col-lg-{{colsLng}}">
                                <div class="btn border-right">
                                    <a href="javascript:void(0)" data-toggle="modal" data-target="#message_popup"><i class="material-icons">message</i>Message</a>
                                </div>
                            </div>
                    {% if ('ROLE_FACILITATOR' in userRole) or ('ROLE_ASSESSOR' in userRole) %}
                                <div class="col-md-{{colsLng}} col-lg-{{colsLng}}">
                                    <div class="btn border-right">
                                        <div class="dropdown keep-open">
                                            <span class="dropdown-toggle" data-toggle="dropdown"><a href="javascript:void(0)"><i class="material-icons">create</i>Note</a></span>
                                            <div class="dropdown-menu">
                                                <div class="data_section">
                                                    <textarea rows="3" placeholder="Notes" id="note-msg" name="note-msg"></textarea>
                                                    <input type="hidden" name="courseId" id="courseId" value="{% if coursePrimaryId >0 %}{{coursePrimaryId}}{% endif %}">
                                                    <div class="date_section clearfix note-msg-inner"></div>
                                                </div>
                                                <button type="button" class="btn_red" id="courseUnitNote" name="courseUnitNote">Send</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-{{colsLng}} col-lg-{{colsLng}}">
                                    <div class="btn border-right">
                                        <div class="dropdown keep-open">
                                            <span class="dropdown-toggle" data-toggle="dropdown"><a href="javascript:void(0)"><i class="material-icons">playlist_add</i>ToDo list</a></span>
                                            <div class="arrow" style="left: 50%;"></div>
                                            <div class="dropdown-menu">
                                                <div class="data_section">
                                                    <textarea rows="3" placeholder="Notes" id="todolist-msg" name="todolist-msg"></textarea>
                                                    <div class="date_section clearfix"><input type="text"  id="remindDate"  class="date_field form-control"/></div>
                                                </div>
                                                <button type="button" class="btn_red setToDoList"  listid="{% if coursePrimaryId >0 %}{{coursePrimaryId}}{% endif %}" usercourseid="{% if coursePrimaryId >0 %}{{coursePrimaryId}}{% endif %}">SET</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                    {% endif %}
                    {% if ('ROLE_ASSESSOR' in userRole) %}
                        <div class="col-md-{{colsLng}} col-lg-{{colsLng}}">
                            <div class="btn"><i class="material-icons">call</i><a href="#">Request CC</a></div>
                        </div>
                    {% endif %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Larg Screens HTML End -->
<!-- Modal -->
<div class="modal fade message_popup" id="message_popup" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-body">
                <div class="new_message_section">
                    <div class="title_bar">NEW MESSAGE<span><i class="material-icons close">close</i></span></div>
                    <div class="new_msg_section">
                        {% set toName = ('ROLE_ASSESSOR' in userRole or 'ROLE_RTO' in userRole) ? facilitatorName : userName %}
                        <form>
                            <div class="input_section">
                                <label class="to">To:</label>
                                {% if 'ROLE_FACILITATOR' in userRole %}
                                    <input type="text" onkeyup="searchUsersFromCourse('#advanced-demo','{{facilitatorId}}','{{assessorId}}','{{rtoId}}','{{userId}}');" value="{{ toName }}" placeholder="" required="required"  name="advanced-demo"  id="advanced-demo" class="ui-autocomplete-input" autocomplete="off" >
                                {% else %}
                                    <input type="text" name="advanced-demo" id="advanced-demo" value="{{ toName }}">
                                {% endif %}
                            </div>
                            <div class="input_section">
                                    <label class="subject">Subject:</label>
                                    <input type="text" name="msgSubject" value="{{courseInfo.name}}" id="msgSubject">
                            </div>
                            <textarea name="msgBody" id="msgBody"></textarea>
                            <input type="hidden" name="UserId" id="UserId" value="{%if('ROLE_ASSESSOR' in userRole or 'ROLE_RTO' in userRole)%}{{facilitatorId}}{% else %}{{userId}}{%endif%}" />
                        </form>
                    </div>
                    <div class="btn_section">
                            <button class="button btn_red msgForCourse" type="button">SUBMIT</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- This modal popup will come when the loggedin user clicks on myprofile link -->
<div class="modal fade profile_popup" id="currProfile" role="dialog">
    <div class="modal-dialog">
		<!-- Modal content-->
        <div class="modal-content">
            <div class="modal-body">
                <span class="close"><i class="material-icons clear_icon">clear</i></span>
                <div class="title_bar hidden-sm hidden-md hidden-lg">
                        <span><i class="material-icons arrow_back">arrow_back</i></span>Your Profile<span><i class="material-icons clear_icon">clear</i></span>
                </div>
                <div class="header_block clearfix">
                    <div class="profile_section">
                        <img class="profile_image" alt="profile_pic" src="{% if 'ROLE_FACILITATOR' in userRole %}{{ facilitatorImage }}{%elseif 'ROLE_ASSESSOR' in userRole%}{{assessorImage}}{% endif %}">
                        <input type="file" class="hidden browse_btn">
                        <div class="content">
                                <h5 class="name"><strong>{{firstName~' '~lastName}}</strong></h5>
                                <span class="field"> {% if 'ROLE_FACILITATOR' in userRole %}Facilitator{%endif%}</span>
                        </div>
                    </div>
                    <div class="count">
                            <span class="number"><strong>17</strong></span>
                            <span class="title">Active Portfolios</span>
                    </div>
                </div>
                    <form class="form">
                            <div class="section name_group clearfix">
                                    <div class="form-group">
                                            <label>First Name</label>
                                            <input type="text" readonly="" value="{{firstName}}">
                                            <span><i class="material-icons">mode_edit</i></span>
                                    </div>
                                    <div class="form-group">
                                            <label>Last Name</label>
                                            <input type="text" readonly="" value="{{lastName}}">
                                            <span><i class="material-icons">mode_edit</i></span>
                                    </div>
                            </div>
                            <div class="section">
                                    <div class="form-group">
                                            <label>Email</label>
                                            <input type="email" readonly="" value="{{email}}">
                                            <span><i class="material-icons">mode_edit</i></span>
                                    </div>
                            </div>
                            <div class="section number_group clearfix">
                                    <div class="form-group">
                                            <label>Mobile</label>
                                            <input type="text" readonly="" value="{{mobile}}">
                                            <span><i class="material-icons">mode_edit</i></span>
                                    </div>
                                    <div class="form-group">
                                            <label>Password</label>
                                            <input type="text" readonly="" value="........">
                                            <span><i class="material-icons">lock</i></span>
                                    </div>
                            </div>
                            <button class="submit_btn" type="submit">SUBMIT</button>
                    </form>
                    <form class="form-inline change_pwd_form">
                            <div class="form-group clearfix">
                                    <label for="email">Current Password</label>
                                    <div class="input_section">
                                            <input type="email" id="email" class="form-control">
                                            <span class="error_message">Please enter current password</span>
                                    </div>
                            </div>
                            <div class="form-group clearfix">
                                    <label for="pwd">Change Password</label>
                                    <div class="input_section">
                                            <input type="password" id="pwd" class="form-control">
                                            <span class="error_message">Please enter current password</span>
                                    </div>
                            </div>
                            <div class="form-group clearfix">
                                    <label for="pwd">Confirm Password</label>
                                    <div class="input_section">
                                            <input type="password" id="pwd" class="form-control">
                                            <span class="error_message">Please enter current password</span>
                                    </div>
                            </div>
                            <div class="btn_section clearfix">
                                    <input type="submit" value="Change Password" class="btn change_pwd_btn">
                                    <input type="button" value="Cancel" class="btn">
                            </div>
                    </form>
            </div>
        </div>
    </div>
</div>

