    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 colxs-12">
            <!-- accordion starts here -->
            <div class="gq-acc-wrap">
                {% if courseInfo.Units %}
                {% if courseInfo.Units.Unit %}
                    {% set flagElective = '0' %}
                    {% set unitsData = courseInfo.Units.Unit %}
                    {% if courseInfo.Units.Unit.id is defined %}
                    {% set unitsData = {0: courseInfo.Units.Unit} %}
                    {% endif %}
                    {% for key, units in unitsData %}
                        {% if units.type == 'elective' %}
                                {% set unitId = units.id %}
                                {% if unitId|trim in electiveUnits|keys %} 
                                {% set flagElective = '1' %}
                                <div>
                                    <input id="acc-{{i}}" name="accordion-1" type="checkbox" />
                                    <div class="row no-margin">
                                        <div id="div_{{units.id}}" class="gq-acc-row-bg">
                                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-2">
                                                <div class="gq-acc-row-title">{{units.id}}</div>
                                            </div>
                                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-4">
                                                <div class="gq-acc-row-title">{{units.name}}</div>
                                            </div>
                                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                                <label for="acc-{{i}}" temp="acc-{{i}}" id="label_{{units.id}}" class="col-xs-4 openIcon btn add-btn upDownArrow view-button" >VIEW</label>
                                            </div>
                                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                            {% set evidence =  evidence_service.getUserUnitEvidences(userId, units.id) %}
                                            {% if evidence %}
                                                <span class="pull-left" id="sp_{{units.id}}"><button id="btneye_{{units.id}}" class="btn eye-btn unit-evidence-id fromnewBottom" unitid="{{units.id}}" course_code="{{courseInfo.id}}" course_name="{{courseInfo.name}}" unittitle="{{units.name}}" del-status="0" userid="{{userId}}" data-toggle="modal" data-target="#edivenceUnitModal" data-direction="bottom"><span class="hidden-xs"></span>SHOW</button></span>
                                                {% if ('ROLE_ASSESSOR' in userRole) or ('ROLE_FACILITATOR' in userRole)  %}
                                                {% if 'ROLE_ASSESSOR' in userRole %}
                                                {% set utype = 'a' %}
                                                {% elseif 'ROLE_FACILITATOR' in userRole %}
                                                {% set utype = 'f' %}
                                                {% endif %} 
                                                <span class="pull-left gq-acc-notes-btn" id=""><button id="notesButton_{{units.id}}" notestype="{{ utype }}" class="btn eye-btn unit-notes fromnewBottom" unitid="{{ user_service.getUnitPrimaryId( userId, units.id, courseInfo.id ) }}" data-toggle="modal" data-target="#edivenceUnitModalNotes" data-direction="bottom"><span class="hidden-xs"></span>NOTES</button></span>
                                                {% endif%}
                                                <div id="{{units.id}}_approveStatus">
                                                {% set approvalStatus = user_service.getUnitStatusByRoleWise( userId, userRole[0], units.id, courseInfo.id ) %}
                                                {% if approvalStatus == 1 %}
                                                    <span class="pull-right gq-acc-book-icon"><img src="{{ asset('public/images')}}/bookopen.png"></span>
                                                {% elseif approvalStatus == 2 %}
                                                    <span class="pull-right gq-acc-book-icon"><img src="{{ asset('public/images')}}/bookclosed.png"></span>
                                                {% endif%}
                                                </div>
                                            {% else %}
                                                <div class="gq-acc-row-title">No Evidences found</div>
                                            {% endif %}
                                            </div>
                                        </div>
                                    </div>
                                    <article class="acc-small">
                                        <p>{{units.name}}</p>
                                    </article>
                                </div>
                                {% set i = i+1 %}
                                {% else %}
                           {% endif %}
                        {% endif %}
                    {% endfor %}
                    {% if flagElective == '0' %}
                            <div class="gq-acc-row-title load-icon-tr">No Elective Units found</div>
                    {% endif %}
                {% endif %}
                {% endif %}
            </div>
            <!-- accordion ends here -->
        </div>
    </div>
    </div>
    {% endif %}
        {% endif %}
     {% endif %}
    
    <div class="col-xs-12 text-center">
        <span class="gq-approved-status" id="status_ar" style="display:none;">Portfolio Submitted!</span>
        <span class="gq-approved-status" id="status_arc" style="display:none;">Issued and Certified!</span>
        <span class="gq-approved-status" id="status_ac" style="display:none;">Certificate Issued!</span>
