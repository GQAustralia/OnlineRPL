{% if userLog %}
    {% for key, list in userLog %}
    <tr>
        <td><a href="javascript:void(0)">{{list.user.firstName}} {{list.user.lastName}}</a></td>
        <td>{{log_list[list.logAction]["action"]}}</td>
        <td>{{list.logDateTime|date("d/m/y h:i")}}</td>
        <!--<td>{{list.logMessage}}</td>-->
    </tr>
    {% endfor %}
{% else %}
        <tr>
            <td colspan="" align="center">No Log Found</td>
			<td></td>
			<td></td>
        </tr>
{% endif %}
{% if userLog|length > 0 and paginator.getTotalPages() > 1 %}
	<tr>
		<td class="gq-pagination-area" colspan="3">
			{# display navigation for pages - paginator_navigator #}        
			{{ include('GqAusUserBundle:Paginator:ajax_paginator_navigator.html.twig', {'paginator' : paginator}) }}  
		</td>
	</tr>
{% endif %}