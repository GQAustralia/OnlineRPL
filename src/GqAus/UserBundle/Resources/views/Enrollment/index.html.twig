                                        <h2 class="page-header">Upload ID Files</h2>    
                                        <div class="form-group">
                                            <section class="upload-control" data-toggle="modal" data-target="#uploadIdFiles" ng-hide="enrollment.upload.uploadId.length > 0">
                                                <div class="upload-control-content">
                                                    <img src="{{ asset('public/ui/img/id.svg') }}" alt="ID" class="upload-icon">
                                                    <p class="upload-text">100 Points of Identification is needed so we're<br/> sure we're recognising the right person</p>
                                                    <button type="button" class="btn btn-link btn-lg">
                                                        <i class="zmdi zmdi-plus-square"></i> Upload your first ID now
                                                    </button>
                                                    <!-- <input class="upload-control-input" type="file" id="uploadID"> -->
                                                </div>
                                            </section>

                                            <section class="consolidated-id-files" ng-show="enrollment.upload.uploadId.length > 0">
                                                <div class="progress-header">
                                                    <div class="progress-tag progress-tag-danger pull-left">
                                                        <progress role="progress" value="{[{getTotalCompleted()}]}" max="100"><img class="fallback" width="30"></progress>
                                                        <figcaption data-value="{[{getTotalCompleted()}]}" data-unit="/100" class="progress-details"> ID POINTS</figcaption>
                                                    </div>

                                                    <button type="button" class="btn btn-link btn-lg pull-right" data-toggle="modal" data-target="#uploadIdFiles"><i class="zmdi zmdi-upload zmdi-hc-lg zmdi-hc-fw"></i> Upload an ID</button>
                                                </div>
                                                <table class="table table-crud">
                                                    <thead>
                                                        <tr>
                                                            <th>ID Points</th>
                                                            <th colspan="2">ID Submitted</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody class="id-files-body">
                                                        <tr ng-repeat="(key, uploadId) in enrollment.upload.uploadId">
                                                            <td><span class="points">{[{uploadId.type.point}]}</span></td>
                                                            <td>
                                                                <div class="progress-tag progress-tag-success" ng-if="uploadId.status != 'completed'">
                                                                    <progress role="progress" value="{[{uploadId.percentageCompleted}]}" max="100"><img class="fallback" width="30"></progress>
                                                                    <figcaption data-value="{[{uploadId.percentageCompleted}]}" data-unit="%" class="progress-details"></figcaption>
                                                                </div>
                                                                <label>{[{uploadId.file.name}]}</label>
                                                                <small class="text-uppercase text-muted">{[{uploadId.type.type}]}</small>
                                                            </td>
                                                            <td class="table-controls">
                                                                <!-- Display this if item is being uploaded -->
                                                                <button type="button" class="btn btn-default btn-xs" ng-if="uploadId.status != 'completed'" ng-click="cancelUpload(key)">
                                                                    <i class="zmdi zmdi-close zmdi-hc-lg"></i> Cancel Upload
                                                                </button>
                                                                <!-- Display this if item is being uploaded -->

                                                                <!-- Only display this if file's already in the server -->
                                                                <button type="button" class="btn btn-link" ng-if="uploadId.status == 'completed'" ng-click="removeUpload(key)"><i class="zmdi zmdi-delete zmdi-hc-lg"></i> Remove</button>
                                                                <!-- Only display this if file's already in the server -->
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </section>

                                        </div>
                                    </fieldset>


                                    <!-- Previous and Next Btn's Starts here-->
                                    <div class="btn-auto-group">
