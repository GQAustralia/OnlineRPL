                                                                        <i class="pseudo-input"><input type="text" maxlength="1" ng-model="enrollment.employment.usiPart[1]" ></i>
                                                                        <i class="pseudo-input"><input type="text" maxlength="1" ng-model="enrollment.employment.usiPart[2]" ></i>
                                                                        <i class="pseudo-input"><input type="text" maxlength="1" ng-model="enrollment.employment.usiPart[3]" ></i>
                                                                        <i class="pseudo-input"><input type="text" maxlength="1" ng-model="enrollment.employment.usiPart[4]" ></i>
                                                                        <i class="pseudo-input"><input type="text" maxlength="1" ng-model="enrollment.employment.usiPart[5]" ></i>
