{% extends 'revamp_base.html.twig' %}
{% block ngApp %} ng-app="gqAus" {% endblock%}
{% block ngController %} ng-controller="enrollmentCtlr"  id="enrollmentCtlr" {% endblock%}
{% block title %}  Enrolment - Your Profile {% endblock %}
{% block ngtitle %}pageTitle{% endblock%}
{% block body %}
    <main class="main-content-gqa">
        <div class="container">
            <!-- Put content here -->
            <div class="panel panel-default">
                <div class="panel-body">
                    <!-- widget div-->
                    <span ng-if="IsLoaded === false"> Loading .. </span>
                    <form id="formWizard" novalidate="novalidate" ng-show="IsLoaded">
                        <div id="formWizardCarousel" class="carousel slide" data-ride="carousel" data-interval="false">
                            <div class="wizard-steps clearfix" id="wizardSteps">
                                <div class="form-bootstrap-wizard">
                                    <ul class="bootstrapWizard form-wizard carousel-indicators">
                                        <li ng-click="formSlideTo(0)" ng-class="{ 'active': (activeForm == '0'), 'done' : completedForms[0]}">
                                            <a href="javascript:void(0)"> <span class="step">1</span> <span class="title">Your Profile</span> </a>
                                        </li>
                                        <li ng-click="formSlideTo(1)" ng-class="{ 'active': (activeForm == 1), 'done' : completedForms[1]}">
                                            <a href="javascript:void(0)"> <span class="step">2</span> <span class="title">Language and <br>Diversity</span> </a>
                                        </li>
                                        <li ng-click="formSlideTo(2)" ng-class="{ 'active': (activeForm == 2), 'done' : completedForms[2]}">
                                            <a href="javascript:void(0)"> <span class="step">3</span> <span class="title">Schooling and <br>Achievements</span> </a>
                                        </li>
                                        <li ng-click="formSlideTo(3)" ng-class="{ 'active': (activeForm == 3), 'done' : completedForms[3]}">
                                            <a href="javascript:void(0)"> <span class="step">4</span> <span class="title">Employment <br> and USI</span> </a>
                                        </li>
                                        <li ng-click="formSlideTo(4)" ng-class="{ 'active': (activeForm == 4), 'done' : completedForms[4]}">
                                            <a href="javascript:void(0)"> <span class="step">5</span> <span class="title">Upload ID files</span> </a>
                                        </li>
                                    </ul>
                                </div>
                            </div>

                            <div class="carousel-inner">

                                <!-- 1st Page Starts Here -->
                                <div class="item active" ng-form name="profileForm">
                                    {#                                    <div ng-bind="profileForm.$error"></div>
                                                                        <ul>
                                                                            <li ng-repeat="(key, errors) in profileForm.$error track by $index"> <strong>{[{ key }]}</strong> errors
                                                                                <ul>
                                                                                    <li ng-repeat="e in errors">{[{ e.$name }]} has an error: <strong>{[{ key }]}</strong>.</li>
                                                                                </ul>
                                                                            </li>
                                                                        </ul>#}
                                    <!-- Personal Details Section STARTS here-->
                                    <fieldset>
                                        <h2 class="page-header">Your Personal Details</h2>
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group" ng-class="{ 'has-error' : profileForm.firstName.$invalid}">
                                                    <label for="firstName" class="control-label">First name</label>
                                                    <input type="text" id="firstName" name="firstName" ng-model="enrollment.profile.firstName" ng-required="true" class="form-control form-control-danger">
                                                    <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.firstName.$invalid " data-toggle="tooltip" data-placement="top" title="First Name is required"></i>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group" ng-class="{ 'has-error' : profileForm.lastName.$invalid}">
                                                    <label for="lastName" class="control-label">Last name</label>
                                                    <input type="text" id="lastName" name="lastName" ng-model="enrollment.profile.lastName" ng-required="true" class="form-control">
                                                    <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.lastName.$invalid " data-toggle="tooltip" data-placement="top" title="Last Name is required"></i>
                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group" ng-class="{ 'has-error' : profileForm.birthday.$invalid}">
                                                    <label for="birthday" class="control-label">Date of birth</label>
                                                    <input type="text" id="birthday" name="birthday" data-init="datepicker" date-validator ng-model="enrollment.profile.birthday" ng-required="true" date-picker="enrollment.profile.birthday" placeholder="DD/MM/YYYY" class="form-control date">
                                                    <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.birthday.$invalid " data-toggle="tooltip" data-placement="top" title="Birthday is required"></i>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group" ng-class="{ 'has-error' : enrollment.profile.gender === '' || enrollment.profile.gender === undefined}">
                                                    <label class="control-label">Gender</label> <br>
                                                    <div class="btn-group  btn-group-sm" data-toggle="buttons">
                                                        <label class="btn btn-default" for="gender" ng-model="enrollment.profile.gender" ng-required="true" uib-btn-radio="'male'">
                                                            <input type="radio" name="gender" > <span>Male</span> </label>
                                                        <label class="btn btn-default" for="gender" ng-model="enrollment.profile.gender" ng-required="true" uib-btn-radio="'female'">
                                                            <input type="radio" name="gender"> <span>Female</span> </label>
                                                        <label class="btn btn-default" for="gender" ng-model="enrollment.profile.gender" ng-required="true" uib-btn-radio="'others'">
                                                            <input type="radio" name="gender"> <span>Others</span> </label>
                                                    </div>
                                                    <i class="zmdi zmdi-alert-triangle" ng-show="enrollment.profile.gender == '' || enrollment.profile.gender === undefined" data-toggle="tooltip" data-placement="top" title="Gender is required"></i>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group" ng-class="{ 'has-error' : profileForm.homeTelNumber.$invalid}">
                                                    <label for="homeTelNumber" class="control-label">Home Telephone Number</label>
                                                    <input type="text" id="homeTelNumber" ng-pattern="/^(\+([ 0-9-\(\)\[\]])*)$/i" name="homeTelNumber" ng-model="enrollment.profile.homeTelNumber" ng-required="true" phoneformat class="form-control" ng-intl-tel-input initial-country="au" placeholder="">
                                                <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.homeTelNumber.$invalid " data-toggle="tooltip" data-placement="top" title="Invalid telephone number"></i>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group" ng-class="{ 'has-error' : profileForm.workTelNumber.$invalid}">
                                                    <label for="workTelNumber" class="control-label">Work telephone number</label>
                                                    <input type="text" id="workTelNumber" name="workTelNumber" ng-pattern="/^(\+([ 0-9-\(\)\[\]])*)$/i" ng-model="enrollment.profile.workTelNumber" ng-required="true" phoneformat class="form-control" ng-intl-tel-input initialCountry="au">
                                                    <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.workTelNumber.$invalid " data-toggle="tooltip" data-placement="top" title="Invalid telephone number"></i>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group" ng-class="{ 'has-error' : profileForm.mobileNumber.$invalid}">
                                                    <label for="mobileNumber" class="control-label">Mobile number</label>
                                                    <input type="text" id="mobileNumber" name="mobileNumber" ng-pattern="/^(\+([ 0-9-\(\)\[\]])*)$/i" ng-model="enrollment.profile.mobileNumber" ng-required="true" phoneformat class="form-control" ng-intl-tel-input initialCountry="au">
                                                    <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.mobileNumber.$invalid " data-toggle="tooltip" data-placement="top" title="Invalid telephone number"></i>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group" ng-class="{ 'has-error' : profileForm.email.$invalid}">
                                                    <label for="email" class="control-label">Email Address</label>
                                                    <input type="email" id="email" name="email" ng-model="enrollment.profile.email" ng-required="true" ng-pattern = '/^(([^<>()\[\]\.,;:\s@\"]+(\.[^<>()\[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()[\]\.,;:\s@\"]+\.)+[^<>()[\]\.,;:\s@\"]{2,})$/i' class="form-control">
                                                    <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.email.$invalid " data-toggle="tooltip" data-placement="top" title="Enter valid email address"></i>
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                    <!-- Personal Details Section END's Here -->

                                    <!-- Address Section STARTS Hear-->
                                    <fieldset>

                                        <div id="mainAddress">
                                            <h2 class="page-header">Your Address</h2>
                                            <div class="row">
                                                <div class="col-xs-12">
                                                    <div class="form-group">
                                                        <p class="text-center">Type and search for your address and we'll automatically fill the details below.</p>
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="zmdi zmdi-pin"></i></span>
                                                            <input id="autocomplete" placeholder="Search city, state, suburb or postcode" class="form-control input-md" type="text">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="divider col-md-12 col-xs-12">
                                                    <p>OR</p>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.propertyName.$invalid}">
                                                        <label for="propertyName" class="control-label">Building / Property name</label>
                                                        <input type="text" id="propertyName" name="propertyName" ng-model="enrollment.profile.address.propertyName" ng-required="true" class="form-control">
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.propertyName.$invalid " data-toggle="tooltip" data-placement="top" title="Property Name is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.unitDetails.$invalid}">
                                                        <label for="unitDetails" class="control-label">Flat / Unit details</label>
                                                        <input type="text" id="unitDetails" name="unitDetails" ng-model="enrollment.profile.address.unitDetails" ng-required="true" class="form-control">
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.unitDetails.$invalid " data-toggle="tooltip" data-placement="top" title="Flat / Unit details is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.street_number.$invalid}">
                                                        <label for="street_number" class="control-label">Street or lot number</label>
                                                        <input type="text" id="street_number" name="street_number" ng-model="enrollment.profile.address.street_number" ng-required="true" class="form-control">
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.street_number.$invalid " data-toggle="tooltip" data-placement="top" title="Street or lot number is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.route.$invalid}">
                                                        <label for="streetName" class="control-label">Street name</label>
                                                        <input type="text" id="route" name="route" ng-model="enrollment.profile.address.route" ng-required="true" class="form-control">
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.route.$invalid " data-toggle="tooltip" data-placement="top" title="Street name is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.locality.$invalid}">
                                                        <label for="suburb" class="control-label">Suburb, locality or town</label>
                                                        <input type="text" id="locality" name="locality" ng-model="enrollment.profile.address.locality" ng-required="true" class="form-control">
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.locality.$invalid " data-toggle="tooltip" data-placement="top" title="Suburb, locality or town is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.state.$invalid}">
                                                        <label for="state" class="control-label">State / territory</label>

                                                        <select class="form-control" id ="state" name ="state" ng-model="enrollment.profile.address.state" ng-required="true" ng-options="i as i for i in selectState(enrollment.profile.address.country)">
                                                            <option value="">Select State</option>
                                                        </select>
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.state.$invalid " data-toggle="tooltip" data-placement="top" title="State is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.postal_code.$invalid}">
                                                        <label for="postcode" class="control-label">Postcode</label>
                                                        <input type="text" id="postal_code" name="postal_code" ng-model="enrollment.profile.address.postal_code" ng-required="true" class="form-control">
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.postal_code.$invalid " data-toggle="tooltip" data-placement="top" title="Postcode is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.country.$invalid}">
                                                        <label for="country" class="control-label">Country</label>
                                                        <select class="form-control" id="country" name ="country" ng-model="enrollment.profile.address.country" ng-required="true" ng-options="i as i for i in countries">
                                                            <option value="">Select Country</option>
                                                        </select>
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.country.$invalid " data-toggle="tooltip" data-placement="top" title="Country is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12">
                                                    <div class="form-group">
                                                        <input type="checkbox" checked value="1" id="postalAddress" ng-true-value="1" ng-false-value="0" name="postalAddress" ng-model="enrollment.profile.postalAddress" class="custom-input"><label for="postalAddress"> My Postal Address is same as above</label>
                                                    </div>                                                
                                                </div>
                                            </div>
                                        </div>

                                    </fieldset>
                                    <!-- Address Section END'S Hear-->
                                    <fieldset>
                                        <div id="postalAddressForm" ng-show="enrollment.profile.postalAddress == '0'">
                                            <h2 class="page-header">Your Postal Address</h2>
                                            <div class="row">
                                                <div class="col-xs-12">
                                                    <div class="form-group">
                                                        <p class="text-center">Type and search for your address and we'll automatically fill the details below.</p>
                                                        <div class="input-group">
                                                            <span class="input-group-addon"><i class="zmdi zmdi-pin"></i></span>
                                                            <input id="autocomplete_postal" placeholder="Search city, state, suburb or postcode" class="form-control input-md" type="text">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="divider col-md-12 col-xs-12">
                                                    <p>OR</p>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.propertyName_postal.$invalid}">
                                                        <label for="propertyName" class="control-label">Building / Property name</label>
                                                        <input type="text" id="propertyName_postal" name="propertyName_postal" ng-model="enrollment.profile.postal.propertyName" ng-required="!enrollment.profile.postalAddress" class="form-control">
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.propertyName_postal.$invalid " data-toggle="tooltip" data-placement="top" title="Building / Property name is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.unitDetails_postal.$invalid}">
                                                        <label for="unitDetails" class="control-label">Flat / Unit details</label>
                                                        <input type="text" id="unitDetails_postal" name="unitDetails_postal" ng-model="enrollment.profile.postal.unitDetails" ng-required="!enrollment.profile.postalAddress" class="form-control">
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.unitDetails_postal.$invalid " data-toggle="tooltip" data-placement="top" title="Flat / Unit details is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.street_number_postal.$invalid}">
                                                        <label for="street_number" class="control-label">Street or lot number</label>
                                                        <input type="text" id="street_number_postal" name="street_number_postal" ng-model="enrollment.profile.postal.street_number" ng-required="!enrollment.profile.postalAddress" class="form-control">
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.street_number_postal.$invalid " data-toggle="tooltip" data-placement="top" title="Street or lot number is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.route_postal.$invalid}">
                                                        <label for="streetName" class="control-label">Street name</label>
                                                        <input type="text" id="route_postal" name="route_postal" ng-model="enrollment.profile.postal.route" ng-required="!enrollment.profile.postalAddress" class="form-control">
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.route_postal.$invalid " data-toggle="tooltip" data-placement="top" title="Street name is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.locality_postal.$invalid}">
                                                        <label for="suburb" class="control-label">Suburb, locality or town</label>
                                                        <input type="text" id="locality_postal" name="locality_postal" ng-model="enrollment.profile.postal.locality" ng-required="!enrollment.profile.postalAddress" class="form-control">
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.locality_postal.$invalid " data-toggle="tooltip" data-placement="top" title="Suburb, locality or town is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.state_postal.$invalid}">
                                                        <label for="state" class="control-label">State / territory</label>
                                                        <input type="hidden" class="form-control" id ="administrative_area_level_1_postal"></select>
                                                        <select class="form-control" id ="state_postal"  name="state_postal" ng-model="enrollment.profile.postal.state" ng-required="!enrollment.profile.postalAddress" ng-options="i as i for i in selectState(enrollment.profile.address.country)">
                                                            <option value="">Select State</option>
                                                        </select>
                                                        <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.state_postal.$invalid " data-toggle="tooltip" data-placement="top" title="State / territory is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.postal_code_postal.$invalid}">
                                                        <label for="postcode" class="control-label">postcode</label>
                                                        <input type="text" id="postal_code_postal" name="postal_code_postal" ng-model="enrollment.profile.postal.postal_code" ng-required="!enrollment.profile.postalAddress" class="form-control">
                                                    <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.postal_code_postal.$invalid " data-toggle="tooltip" data-placement="top" title="postcode is required"></i>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group" ng-class="{ 'has-error' : profileForm.country_postal.$invalid}">
                                                        <label for="country" class="control-label">Country</label>
                                                        <select class="form-control" id="country_postal" name="country_postal" ng-model="enrollment.profile.postal.country" ng-required="!enrollment.profile.postalAddress" ng-options="i as i for i in countries">
                                                            <option value="">Select Country</option>
                                                        </select>
                                                    <i class="zmdi zmdi-alert-triangle" ng-show="profileForm.country_postal.$invalid " data-toggle="tooltip" data-placement="top" title="Country is required"></i>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                    <!-- Previous and Next Btn's Starts here-->

                                    <div class="btn-auto-group">
                                        <button type="button" class="btn btn-primary pull-right"  ng-disabled="profileForm.$invalid" ng-click="profileForm.$valid && proceedNext(0)">Next: Language and Diversity <i class="zmdi zmdi-chevron-right zmdi-hc-lg"></i></button>
                                    </div>
                                    <!-- Previous and Next Btn's Ends here-->
                                </div>
                                <!-- 1st Page Ends Here -->

                                <!-- 2nd Page Starts Here -->
                                <div class="item" ng-form name="languageForm">
                                    <!-- Language and cultural diversity Section STARTS here-->
                                    <fieldset>
                                        <h2 class="page-header">Language and cultural diversity</h2>
                                        {#      <div ng-bind="languageForm.$error"></div>
                                              <ul>
                                                  <li ng-repeat="(key, errors) in languageForm.$error track by $index"> <strong>{[{ key }]}</strong> errors
                                                      <ul>
                                                          <li ng-repeat="e in errors">{[{ e.$name }]} has an error: <strong>{[{ key }]}</strong>.</li>
                                                      </ul>
                                                  </li>
                                              </ul>#}
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6">
                                                <div class="form-group" ng-class="{ 'has-error' : languageForm.country2.$invalid}">
                                                    <p><label for="country">In which country were you born?</label></p>
                                                    <select class="form-control" id="country2" name ="country2" ng-model="enrollment.language.country" ng-required="true" ng-options="i as i for i in countries">
                                                        <option value="">Select Country</option>
                                                    </select>
                                                     <i class="zmdi zmdi-alert-triangle" ng-show="languageForm.country2.$invalid " data-toggle="tooltip" data-placement="top" title="Country is required"></i>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group" ng-class="{ 'has-error' : enrollment.language.speakOther === '' || enrollment.language.speakOther === undefined}">
                                                    <p><label>Do you speak a language other than English at home?</label></p>
                                                    <div class="btn-group btn-group-sm" data-toggle="buttons">
                                                        
                                                        <label class="btn btn-default" for="langauge" data-collapse="hide" data-target="#LanguageSpoken" ng-model="enrollment.language.speakOther" ng-required="true" uib-btn-radio="0">
                                                            <input type="radio" name="langauge">
