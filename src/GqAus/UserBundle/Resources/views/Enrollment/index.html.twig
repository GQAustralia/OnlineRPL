{% extends "::enrollment.html.twig" %}
{% block body %}
    <main class="main-content-gqa" ng-app = "gqAus">
        <div class="container" id="enrollmentCtlr" ng-controller="enrollmentCtlr">
            <!-- Put content here -->
            <div class="panel panel-default">
                <div class="panel-body">
                    <!-- widget div-->
                    <form id="formWizard" novalidate="novalidate">
                        <div id="formWizardCarousel" class="carousel slide" data-ride="carousel" data-interval="false">
                            <div class="wizard-steps clearfix" id="wizardSteps">
                                <div class="form-bootstrap-wizard">
                                    <ul class="bootstrapWizard form-wizard">
                                        <li class="active" data-target="#formWizardCarousel" data-slide-to="0" class="active">
                                            <a href="#tab1" data-toggle="tab" class="active"> <span class="step">1</span> <span class="title">Your Profile</span> </a>
                                        </li>
                                        <li class="" ng-disabled="profileForm.$invalid" data-target="#formWizardCarousel" data-slide-to="1">
                                            <a href="#tab2" data-toggle="tab"> <span class="step">2</span> <span class="title">Language and <br>Diversity</span> </a>
                                        </li>
                                        <li class="" data-target="#formWizardCarousel" data-slide-to="2">
                                            <a href="#tab3" data-toggle="tab"> <span class="step">3</span> <span class="title">Schooling and <br>Achievements</span> </a>
                                        </li>
                                        <li class="" data-target="#formWizardCarousel" data-slide-to="3">
                                            <a href="#tab4" data-toggle="tab"> <span class="step">4</span> <span class="title">Employment <br> and USI</span> </a>
                                        </li>
                                        <li class="" data-target="#formWizardCarousel" data-slide-to="4">
                                            <a href="#tab5" data-toggle="tab"> <span class="step">5</span> <span class="title">Upload ID files</span> </a>
                                        </li>
                                    </ul>
                                </div>
                            </div>

                            <div class="carousel-inner">

                                <!-- 1st Page Starts Here -->
                                <div class="item active" ng-form name="profileForm">
{#                                    <div ng-bind="profileForm.$error"></div>
                                    <ul>
                                        <li ng-repeat="(key, errors) in profileForm.$error track by $index"> <strong>{[{ key }]}</strong> errors
                                            <ul>
                                                <li ng-repeat="e in errors">{[{ e.$name }]} has an error: <strong>{[{ key }]}</strong>.</li>
                                            </ul>
                                        </li>
                                    </ul>#}
                                    <!-- Personal Details Section STARTS here-->
                                    <fieldset>
                                        <h2 class="page-header">Your Personal Details</h2>
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="firstName" class="control-label">First Name</label>
                                                    <input type="text" id="firstName" name="firstName" ng-model="enrollment.profile.firstName" ng-required="true" class="form-control form-control-danger">
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="lastName" class="control-label">Last Name</label>
                                                    <input type="text" id="lastName" name="lastName" ng-model="enrollment.profile.lastName" ng-required="true" class="form-control">
                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="birthday" class="control-label">Date of Birth</label>
                                                    <input type="text" id="birthday" name="birthday" date-validator ng-model="enrollment.profile.birthday" ng-required="true" date-picker="enrollment.profile.birthday" class="form-control date">
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label class="control-label">Gender</label> <br>
                                                    <div class="btn-group  btn-group-sm" data-toggle="buttons">
                                                        <label class="btn btn-default" for="gender" ng-model="enrollment.profile.gender" ng-required="true" uib-btn-radio="'Male'">
                                                            <input type="radio" name="gender" > <span>Male</span> </label>
                                                        <label class="btn btn-default" for="gender" ng-model="enrollment.profile.gender" ng-required="true" uib-btn-radio="'Female'">
                                                            <input type="radio" name="gender"> <span>Female</span> </label>
                                                        <label class="btn btn-default" for="gender" ng-model="enrollment.profile.gender" ng-required="true" uib-btn-radio="'Others'">
                                                            <input type="radio" name="gender"> <span>Others</span> </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="homeTelNumber" class="control-label">Home Telephone Number</label>
                                                    <input type="text" id="homeTelNumber" name="homeTelNumber" ng-model="enrollment.profile.homeTelNumber" ng-required="true" phoneformat class="form-control" ng-intl-tel-input initial-country="au" placeholder="">
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="workTelNumber" class="control-label">Work Telephone Number</label>
                                                    <input type="text" id="workTelNumber" name="workTelNumber" ng-model="enrollment.profile.workTelNumber" ng-required="true" phoneformat class="form-control" ng-intl-tel-input initialCountry="au">
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="mobileNumber" class="control-label">Mobile Number</label>
                                                    <input type="text" id="mobileNumber" name="mobileNumber" ng-model="enrollment.profile.mobileNumber" ng-required="true" phoneformat class="form-control" ng-intl-tel-input initialCountry="au">
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="email" class="control-label">Email Address</label>
                                                    <input type="email" id="email" name="email" ng-model="enrollment.profile.email" ng-required="true" class="form-control">
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                    <!-- Personal Details Section END's Here -->

                                    <!-- Address Section STARTS Hear-->
                                    <fieldset>
                                        <h2 class="page-header">Your Address</h2>

                                        <div class="row">
                                            <div id="mainAddress">
                                                <div class="col-xs-12">
                                                    <div class="form-group">
                                                        <small class="text-muted">Type and search for your address and we'll automatically fill the details below. You can modify them manually if you need to.</small>
                                                        <input id="autocomplete" placeholder="Search city, state, suburb or postcode" class="form-control input-lg" type="text">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="propertyName" class="control-label">Building / Property Name</label>
                                                        <input type="text" id="propertyName" name="propertyName" ng-model="enrollment.profile.address.propertyName" ng-required="true" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="unitDetails" class="control-label">Flat / Unit details</label>
                                                        <input type="text" id="unitDetails" name="unitDetails" ng-model="enrollment.profile.address.unitDetails" ng-required="true" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="street_number" class="control-label">Street or lot number</label>
                                                        <input type="text" id="street_number" name="street_number" ng-model="enrollment.profile.address.street_number" ng-required="true" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="streetName" class="control-label">Street Name</label>
                                                        <input type="text" id="route" name="route" ng-model="enrollment.profile.address.route" ng-required="true" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="co-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="suburb" class="control-label">Suburb, locality or town</label>
                                                        <input type="text" id="locality" name="locality" ng-model="enrollment.profile.address.locality" ng-required="true" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="state" class="control-label">State / territory</label>
                                                        
                                                        <select class="form-control" id ="state" name ="state" ng-model="enrollment.profile.address.state" ng-required="true" ng-options="i as i for i in selectState(enrollment.profile.address.country)">
                                                            <option value="">Select State</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="postcode" class="control-label">Post Code</label>
                                                        <input type="text" id="postal_code" name="postal_code" ng-model="enrollment.profile.address.postal_code" ng-required="true" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="country" class="control-label">Country</label>
                                                        <select class="form-control" id="country" name ="country" ng-model="enrollment.profile.address.country" ng-required="true" ng-options="i as i for i in countries">
                                                            <option value="">Select Country</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12">
                                                    <div class="form-group">
                                                        <input type="checkbox" checked value="1" id="postalAddress" ng-true-value="true" ng-false-value="false" name="postalAddress" ng-model="enrollment.profile.postalAddress" data-toggle="collapse" data-target="#postalAddressForm" class="custom-input"><label for="postalAddress"> My Postal Address is same as above</label>
                                                    </div>                                                
                                                </div>
                                            </div>
                                        </div>
                                        
                                    </fieldset>
                                    <!-- Address Section END'S Hear-->
                                    <fieldset>
                                        <div id="postalAddressForm" class="collapse">
                                            <h2 class="page-header">Your Postal Address</h2>
                                            <div class="row">



                                                <div class="col-xs-12">
                                                    <div class="form-group">
                                                        <input id="autocomplete_postal" placeholder="Search city, state, suburb or postcode" class="form-control input-lg" type="text">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="propertyName" class="control-label">Building / Property Name</label>
                                                        <input type="text" id="propertyName_postal" name="propertyName_postal" ng-model="enrollment.profile.postal.propertyName" ng-required="!enrollment.profile.postalAddress" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="unitDetails" class="control-label">Flat / Unit details</label>
                                                        <input type="text" id="unitDetails_postal" name="unitDetails_postal" ng-model="enrollment.profile.postal.unitDetails" ng-required="!enrollment.profile.postalAddress" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="street_number" class="control-label">Street or lot number</label>
                                                        <input type="text" id="street_number_postal" name="street_number_postal" ng-model="enrollment.profile.postal.street_number" ng-required="!enrollment.profile.postalAddress" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="streetName" class="control-label">Street Name</label>
                                                        <input type="text" id="route_postal" name="route_postal" ng-model="enrollment.profile.postal.route" ng-required="!enrollment.profile.postalAddress" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="co-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="suburb" class="control-label">Suburb, locality or town</label>
                                                        <input type="text" id="locality_postal" name="locality_postal" ng-model="enrollment.profile.postal.locality" ng-required="!enrollment.profile.postalAddress" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="state" class="control-label">State / territory</label>
                                                        <input type="hidden" class="form-control" id ="administrative_area_level_1_postal"></select>
                                                        <select class="form-control" id ="state_postal"  name="state_postal" ng-model="enrollment.profile.postal.state" ng-required="!enrollment.profile.postalAddress" ng-options="i as i for i in selectState(enrollment.profile.address.country)">
                                                            <option value="">Select State</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="postcode" class="control-label">Post Code</label>
                                                        <input type="text" id="postal_code_postal" name="postal_code_postal" ng-model="enrollment.profile.postal.postal_code" ng-required="!enrollment.profile.postalAddress" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="country" class="control-label">Country</label>
                                                        <select class="form-control" id="country_postal" name="country_postal" ng-model="enrollment.profile.postal.country" ng-required="!enrollment.profile.postalAddress" ng-options="i as i for i in countries">
                                                            <option value="">Select Country</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                    <!-- Previous and Next Btn's Starts here-->
                                    <div class="clearfix">
                                        <button type="button" class="btn btn-primary pull-right" data-target="#formWizardCarousel" data-slide-to="1"  ng-disabled="profileForm.$invalid" ng-click="profileForm.$valid && proceedNext('profile')"><span class="text-uppercase">Next:</span> Language and Diversity <i class="zmdi zmdi-chevron-right zmdi-hc-lg"></i></button>
                                    </div>
                                    <!-- Previous and Next Btn's Ends here-->
                                </div>
                                <!-- 1st Page Ends Here -->

                                <!-- 2nd Page Starts Here -->
                                <div class="item">
                                    <!-- Language and cultural diversity Section STARTS here-->
                                    <fieldset>
                                        <h2 class="page-header">Language and cultural diversity</h2>

                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6">
                                                <div class="form-group">
                                                    <label for="country" class="control-label">In which country were you born?</label>
                                                    <select class="form-control" id="country2" name ="country2"></select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <div class="btn-group btn-group-sm" data-toggle="buttons">
                                                        <p><label>Do you speak a language other than English at home?</label></p>
                                                        <label class="btn btn-default" for="langauge">
                                                            <input type="radio" name="langauge">
                                                            <span>No, English only</span>
                                                        </label>
                                                        <label class="btn btn-default" for="langauge">
                                                            <input type="radio" name="langauge">
                                                            <span>Yes, Please specify</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <div class="btn-group btn-group-sm" data-toggle="buttons">
                                                        <p><label class="clear">How well do you speak English?</label></p>
                                                        <label class="btn btn-default" for="gender">
                                                            <input type="radio" name="gender"> <span>Very well</span>
                                                        </label>
                                                        <label class="btn btn-default" for="gender">
                                                            <input type="radio" name="gender"> <span>Well</span>
                                                        </label>
                                                        <label class="btn btn-default" for="gender">
                                                            <input type="radio" name="gender"> <span>Not well</span>
                                                        </label>
                                                        <label class="btn btn-default" for="gender">
                                                            <input type="radio" name="gender"> <span>Not at all</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <div class="btn-group btn-group-sm" data-toggle="buttons">
                                                        <p><label class="clear">Are you of Aboriginal or Torres Strait Islander origin?</label></p>
                                                        <label class="btn btn-default" for="gender">
                                                            <input type="radio" name="gender"> <span>No</span>
                                                        </label>
                                                        <label class="btn btn-default" for="gender">
                                                            <input type="radio" name="gender"> <span>Yes, Aboriginal</span>
                                                        </label>
                                                        <label class="btn btn-default" for="gender">
                                                            <input type="radio" name="gender"> <span>Yes, Torres Strait Islander</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                    <!-- Langugage and cultural diversity END's Here -->

                                    <!-- Disability Section STARTS Here-->
                                    <fieldset>
                                        <h2 class="page-header">Disability</h2>
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                                <div class="form-group">
                                                    <div class="btn-group btn-group-sm" data-toggle="buttons">
                                                        <p><label class="clear">Do you consider yourself to have a disability, impairment or long-term condition?</label></p>
                                                        <label class="btn btn-default" for="gender" data-toggle="collapse" data-parent="#accordion" href="#DisabilityCollapse">
                                                            <input type="radio" name="gender"> <span>Yes</span>
                                                        </label>
                                                        <label class="btn btn-default" for="gender" data-toggle="collapse" data-parent="#accordion" href="#DisabilityCollapse">
                                                            <input type="radio" name="gender"> <span>No</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- Disability Options STARTS Here-->
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div id="DisabilityCollapse" class="panel-collapse collapse">
                                                    <div class="disability-options">
                                                        <p>Please select from the list below the area(s) of disability, impairment or long-term condition (select as many as applicable):</p>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">

                                                            <div class="form-group">
                                                                <label> <input type="checkbox" name="optionsCheck" id="optionsCheck1" value="option1">Hearing / deaf </label>
                                                            </div>
                                                        </div>
                                                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                            <div class="form-group">
                                                                <label> <input type="checkbox" name="optionsCheck" id="optionsCheck2" value="option2">Acquired brain impairment </label>
                                                            </div>
                                                        </div>
                                                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                            <div class="form-group">
                                                                <label> <input type="checkbox" name="optionsCheck" id="optionsCheck2" value="option2">Physical </label>
                                                            </div>
                                                        </div>
                                                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                            <div class="form-group">
                                                                <label> <input type="checkbox" name="optionsCheck" id="optionsCheck2" value="option2">Vision </label>
                                                            </div>
                                                        </div>
                                                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                            <div class="form-group">
                                                                <label> <input type="checkbox" name="optionsCheck" id="optionsCheck2" value="option2">Intellectual </label>
                                                            </div>
                                                        </div>
                                                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                            <div class="form-group">
                                                                <label> <input type="checkbox" name="optionsCheck" id="optionsCheck2" value="option2">Medical condition </label>
                                                            </div>
                                                        </div>
                                                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                            <div class="form-group">
                                                                <label> <input type="checkbox" name="optionsCheck" id="optionsCheck2" value="option2">Learning </label>
                                                            </div>
                                                        </div>
                                                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                            <div class="form-group">
                                                                <label> <input type="checkbox" name="optionsCheck" id="optionsCheck2" value="option2">Other </label>
                                                            </div>
                                                        </div>
                                                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                            <div class="form-group">
                                                                <label> <input type="checkbox" name="optionsCheck" id="optionsCheck2" value="option2">Mental illness </label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- Disability Options STARTS Here-->
                                        </div>
                                    </fieldset>
                                    <!-- Disability Section END'S Here-->

                                    <!-- Previous and Next Btn's Starts here-->
                                    <div class="btn-auto-group">
                                        <button type="button" class="btn btn-default" data-target="#formWizardCarousel" data-slide-to="0"><i class="zmdi zmdi-chevron-left zmdi-hc-lg"></i> Personal Details </button>

                                        <button type="button" class="btn btn-primary" data-target="#formWizardCarousel" data-slide-to="2">NEXT: Schooling and Achievements <i class="zmdi zmdi-chevron-right zmdi-hc-lg"></i> </button>
                                    </div>
                                    <!-- Previous and Next Btn's Ends here-->
                                </div>
                                <!-- 2nd Page Ends Here -->

                                <!-- 3rd Page Starts Here -->
                                <div class="item">
                                    <!-- Schooling Section STARTS here-->
                                    <fieldset>
                                        <div class="row">
                                            <div class="col-xs-12">
                                                <h2>Schooling</h2>
                                                <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Nullam feugiat vel lacus ut vehicula.</p>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12"><span>What is your highest COMPLETED school level. Choose only one:</span></div>

                                            <div class="col-xs-12 radio">
                                                <div class="form-group">
                                                    <label> <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1">Year 12 or equivalent </label>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 radio">
                                                <div class="form-group">
