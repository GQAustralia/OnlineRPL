{% extends "::enrollment.html.twig" %}
{% block body %}
    <main class="main-content-gqa" ng-app="gqAus">
        <div class="container" id="enrollmentCtlr" ng-controller="enrollmentCtlr">
            <!-- Put content here -->
            <div class="panel panel-default">
                <div class="panel-body">
                    <!-- widget div-->
                    <span ng-if="IsLoaded === false"> Loading .. </span>
                    <form id="formWizard" novalidate="novalidate" ng-show="IsLoaded">
                        <div id="formWizardCarousel" class="carousel slide" data-ride="carousel" data-interval="false">
                            <div class="wizard-steps clearfix" id="wizardSteps">
                                <div class="form-bootstrap-wizard">
                                    <ul class="bootstrapWizard form-wizard">
                                        <li ng-click="formSlideTo(0)" ng-class="{ 'active': (activeForm == '0') }">
                                            <a href="javascript:void(0)"> <span class="step">1</span> <span
                                                        class="title">Your Profile</span> </a>
                                        </li>
                                        <li ng-click="formSlideTo(1)" ng-class="{ 'active': (activeForm == 1) }">
                                            <a href="javascript:void(0)"> <span class="step">2</span> <span
                                                        class="title">Language and <br>Diversity</span> </a>
                                        </li>
                                        <li ng-click="formSlideTo(2)" ng-class="{ 'active': (activeForm == 2) }">
                                            <a href="javascript:void(0)"> <span class="step">3</span> <span
                                                        class="title">Schooling and <br>Achievements</span> </a>
                                        </li>
                                        <li ng-click="formSlideTo(3)" ng-class="{ 'active': (activeForm == 3) }">
                                            <a href="javascript:void(0)"> <span class="step">4</span> <span
                                                        class="title">Employment <br> and USI</span> </a>
                                        </li>
                                        <li ng-click="formSlideTo(4)" ng-class="{ 'active': (activeForm == 4) }">
                                            <a href="javascript:void(0)"> <span class="step">5</span> <span
                                                        class="title">Upload ID files</span> </a>
                                        </li>
                                    </ul>
                                </div>
                            </div>

                            <div class="carousel-inner">

                                <!-- 1st Page Starts Here -->
                                <div class="item active" ng-form name="profileForm">
                                    {#                                    <div ng-bind="profileForm.$error"></div>
                                                                        <ul>
                                                                            <li ng-repeat="(key, errors) in profileForm.$error track by $index"> <strong>{[{ key }]}</strong> errors
                                                                                <ul>
                                                                                    <li ng-repeat="e in errors">{[{ e.$name }]} has an error: <strong>{[{ key }]}</strong>.</li>
                                                                                </ul>
                                                                            </li>
                                                                        </ul>#}
                                    <!-- Personal Details Section STARTS here-->
                                    <fieldset>
                                        <h2 class="page-header">Your Personal Details</h2>
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="firstName" class="control-label">First Name</label>
                                                    <input type="text" id="firstName" name="firstName"
                                                           ng-model="enrollment.profile.firstName" ng-required="true"
                                                           class="form-control form-control-danger">
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="lastName" class="control-label">Last Name</label>
                                                    <input type="text" id="lastName" name="lastName"
                                                           ng-model="enrollment.profile.lastName" ng-required="true"
                                                           class="form-control">
                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="birthday" class="control-label">Date of Birth</label>
                                                    <input type="text" id="birthday" name="birthday" date-validator
                                                           ng-model="enrollment.profile.birthday" ng-required="true"
                                                           date-picker="enrollment.profile.birthday"
                                                           class="form-control date">
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label class="control-label">Gender</label> <br>
                                                    <div class="btn-group  btn-group-sm" data-toggle="buttons">
                                                        <label class="btn btn-default" for="gender"
                                                               ng-model="enrollment.profile.gender" ng-required="true"
                                                               uib-btn-radio="'male'">
                                                            <input type="radio" name="gender"> <span>Male</span>
                                                        </label>
                                                        <label class="btn btn-default" for="gender"
                                                               ng-model="enrollment.profile.gender" ng-required="true"
                                                               uib-btn-radio="'female'">
                                                            <input type="radio" name="gender"> <span>Female</span>
                                                        </label>
                                                        <label class="btn btn-default" for="gender"
                                                               ng-model="enrollment.profile.gender" ng-required="true"
                                                               uib-btn-radio="'others'">
                                                            <input type="radio" name="gender"> <span>Others</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="homeTelNumber" class="control-label">Home Telephone
                                                        Number</label>
                                                    <input type="text" id="homeTelNumber" name="homeTelNumber"
                                                           ng-model="enrollment.profile.homeTelNumber"
                                                           ng-required="true" phoneformat class="form-control"
                                                           ng-intl-tel-input initial-country="au" placeholder="">
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="workTelNumber" class="control-label">Work Telephone
                                                        Number</label>
                                                    <input type="text" id="workTelNumber" name="workTelNumber"
                                                           ng-model="enrollment.profile.workTelNumber"
                                                           ng-required="true" phoneformat class="form-control"
                                                           ng-intl-tel-input initialCountry="au">
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="mobileNumber" class="control-label">Mobile
                                                        Number</label>
                                                    <input type="text" id="mobileNumber" name="mobileNumber"
                                                           ng-model="enrollment.profile.mobileNumber" ng-required="true"
                                                           phoneformat class="form-control" ng-intl-tel-input
                                                           initialCountry="au">
                                                </div>
                                            </div>
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <label for="email" class="control-label">Email Address</label>
                                                    <input type="email" id="email" name="email"
                                                           ng-model="enrollment.profile.email" ng-required="true"
                                                           class="form-control">
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                    <!-- Personal Details Section END's Here -->

                                    <!-- Address Section STARTS Hear-->
                                    <fieldset>

                                        <div id="mainAddress">
                                            <h2 class="page-header">Your Address</h2>
                                            <div class="row">
                                                <div class="col-xs-12">
                                                    <div class="form-group">
                                                        <small class="text-muted">Type and search for your address and
                                                            we'll automatically fill the details below. You can modify
                                                            them manually if you need to.
                                                        </small>
                                                        <input id="autocomplete"
                                                               placeholder="Search city, state, suburb or postcode"
                                                               class="form-control input-lg" type="text">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="propertyName" class="control-label">Building /
                                                            Property Name</label>
                                                        <input type="text" id="propertyName" name="propertyName"
                                                               ng-model="enrollment.profile.address.propertyName"
                                                               ng-required="true" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="unitDetails" class="control-label">Flat / Unit
                                                            details</label>
                                                        <input type="text" id="unitDetails" name="unitDetails"
                                                               ng-model="enrollment.profile.address.unitDetails"
                                                               ng-required="true" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="street_number" class="control-label">Street or lot
                                                            number</label>
                                                        <input type="text" id="street_number" name="street_number"
                                                               ng-model="enrollment.profile.address.street_number"
                                                               ng-required="true" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="streetName" class="control-label">Street
                                                            Name</label>
                                                        <input type="text" id="route" name="route"
                                                               ng-model="enrollment.profile.address.route"
                                                               ng-required="true" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="suburb" class="control-label">Suburb, locality or
                                                            town</label>
                                                        <input type="text" id="locality" name="locality"
                                                               ng-model="enrollment.profile.address.locality"
                                                               ng-required="true" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="state" class="control-label">State /
                                                            territory</label>

                                                        <select class="form-control" id="state" name="state"
                                                                ng-model="enrollment.profile.address.state"
                                                                ng-required="true"
                                                                ng-options="i as i for i in selectState(enrollment.profile.address.country)">
                                                            <option value="">Select State</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="postcode" class="control-label">Post Code</label>
                                                        <input type="text" id="postal_code" name="postal_code"
                                                               ng-model="enrollment.profile.address.postal_code"
                                                               ng-required="true" class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="country" class="control-label">Country</label>
                                                        <select class="form-control" id="country" name="country"
                                                                ng-model="enrollment.profile.address.country"
                                                                ng-required="true"
                                                                ng-options="i as i for i in countries">
                                                            <option value="">Select Country</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12">
                                                    <div class="form-group">
                                                        <input type="checkbox" checked value="1" id="postalAddress"
                                                               ng-true-value="1" ng-false-value="0" name="postalAddress"
                                                               ng-model="enrollment.profile.postalAddress"
                                                               class="custom-input"><label for="postalAddress"> My
                                                            Postal Address is same as above</label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </fieldset>
                                    <!-- Address Section END'S Hear-->
                                    <fieldset>
                                        <div id="postalAddressForm" ng-show="enrollment.profile.postalAddress == '0'">
                                            <h2 class="page-header">Your Postal Address</h2>
                                            <div class="row">


                                                <div class="col-xs-12">
                                                    <div class="form-group">
                                                        <input id="autocomplete_postal"
                                                               placeholder="Search city, state, suburb or postcode"
                                                               class="form-control input-lg" type="text">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="propertyName" class="control-label">Building /
                                                            Property Name</label>
                                                        <input type="text" id="propertyName_postal"
                                                               name="propertyName_postal"
                                                               ng-model="enrollment.profile.postal.propertyName"
                                                               ng-required="!enrollment.profile.postalAddress"
                                                               class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="unitDetails" class="control-label">Flat / Unit
                                                            details</label>
                                                        <input type="text" id="unitDetails_postal"
                                                               name="unitDetails_postal"
                                                               ng-model="enrollment.profile.postal.unitDetails"
                                                               ng-required="!enrollment.profile.postalAddress"
                                                               class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="street_number" class="control-label">Street or lot
                                                            number</label>
                                                        <input type="text" id="street_number_postal"
                                                               name="street_number_postal"
                                                               ng-model="enrollment.profile.postal.street_number"
                                                               ng-required="!enrollment.profile.postalAddress"
                                                               class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="streetName" class="control-label">Street
                                                            Name</label>
                                                        <input type="text" id="route_postal" name="route_postal"
                                                               ng-model="enrollment.profile.postal.route"
                                                               ng-required="!enrollment.profile.postalAddress"
                                                               class="form-control">
                                                    </div>
                                                </div>
                                                <div class="co-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="suburb" class="control-label">Suburb, locality or
                                                            town</label>
                                                        <input type="text" id="locality_postal" name="locality_postal"
                                                               ng-model="enrollment.profile.postal.locality"
                                                               ng-required="!enrollment.profile.postalAddress"
                                                               class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="state" class="control-label">State /
                                                            territory</label>
                                                        <input type="hidden" class="form-control"
                                                               id="administrative_area_level_1_postal"></select>
                                                        <select class="form-control" id="state_postal"
                                                                name="state_postal"
                                                                ng-model="enrollment.profile.postal.state"
                                                                ng-required="!enrollment.profile.postalAddress"
                                                                ng-options="i as i for i in selectState(enrollment.profile.address.country)">
                                                            <option value="">Select State</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="postcode" class="control-label">Post Code</label>
                                                        <input type="text" id="postal_code_postal"
                                                               name="postal_code_postal"
                                                               ng-model="enrollment.profile.postal.postal_code"
                                                               ng-required="!enrollment.profile.postalAddress"
                                                               class="form-control">
                                                    </div>
                                                </div>
                                                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                    <div class="form-group">
                                                        <label for="country" class="control-label">Country</label>
                                                        <select class="form-control" id="country_postal"
                                                                name="country_postal"
                                                                ng-model="enrollment.profile.postal.country"
                                                                ng-required="!enrollment.profile.postalAddress"
                                                                ng-options="i as i for i in countries">
                                                            <option value="">Select Country</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                    <!-- Previous and Next Btn's Starts here-->
                                    <div class="clearfix">
                                        <button type="button" class="btn btn-primary pull-right"
                                                ng-disabled="profileForm.$invalid"
                                                ng-click="profileForm.$valid && proceedNext(0)"><span
                                                    class="text-uppercase">Next:</span> Language and Diversity <i
                                                    class="zmdi zmdi-chevron-right zmdi-hc-lg"></i></button>
                                    </div>
                                    <!-- Previous and Next Btn's Ends here-->
                                </div>
                                <!-- 1st Page Ends Here -->

                                <!-- 2nd Page Starts Here -->
                                <div class="item" ng-form name="languageForm">
                                    <!-- Language and cultural diversity Section STARTS here-->
                                    <fieldset>
                                        <h2 class="page-header">Language and cultural diversity</h2>
                                        {#      <div ng-bind="languageForm.$error"></div>
                                              <ul>
                                                  <li ng-repeat="(key, errors) in languageForm.$error track by $index"> <strong>{[{ key }]}</strong> errors
                                                      <ul>
                                                          <li ng-repeat="e in errors">{[{ e.$name }]} has an error: <strong>{[{ key }]}</strong>.</li>
                                                      </ul>
                                                  </li>
                                              </ul>#}
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6">
                                                <div class="form-group">
                                                    <label for="country" class="control-label">In which country were you
                                                        born?</label>
                                                    <select class="form-control" id="country2" name="country2"
                                                            ng-model="enrollment.language.country" ng-required="true"
                                                            ng-options="i as i for i in countries">
                                                        <option value="">Select Country</option>
                                                    </select>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <div class="btn-group btn-group-sm" data-toggle="buttons">
                                                        <p><label>Do you speak a language other than English at
                                                                home?</label></p>
                                                        <label class="btn btn-default" for="langauge"
                                                               data-collapse="hide" data-target="#LanguageSpoken"
                                                               ng-model="enrollment.language.speakOther"
                                                               ng-required="true" uib-btn-radio="0">
                                                            <input type="radio" name="langauge">
                                                            <span>No, English only</span>
                                                        </label>
                                                        <label class="btn btn-default" for="langauge"
                                                               ng-model="enrollment.language.speakOther"
                                                               ng-required="true" uib-btn-radio="1">
                                                            <input type="radio" name="langauge">
                                                            <span>Yes, Please specify</span>
                                                        </label>

                                                        <div id="LanguageSpoken"
                                                             ng-show="enrollment.language.speakOther == 1">
                                                            <input name="language" type="text" class="form-control"
                                                                   ng-model="enrollment.language.speakLanguage"
                                                                   ng-required="enrollment.language.speakOther == 1">
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <div class="btn-group btn-group-sm" data-toggle="buttons">
                                                        <p><label class="clear">How well do you speak English?</label>
                                                        </p>
                                                        <label class="btn btn-default" for="speakEnglish"
                                                               ng-model="enrollment.language.speakEnglish"
                                                               ng-required="true" uib-btn-radio="'Very Well'">
                                                            <input type="radio" name="speakEnglish">
                                                            <span>Very well</span>
                                                        </label>
                                                        <label class="btn btn-default" for="speakEnglish"
                                                               ng-model="enrollment.language.speakEnglish"
                                                               ng-required="true" uib-btn-radio="'Well'">
                                                            <input type="radio" name="speakEnglish"> <span>Well</span>
                                                        </label>
                                                        <label class="btn btn-default" for="speakEnglish"
                                                               ng-model="enrollment.language.speakEnglish"
                                                               ng-required="true" uib-btn-radio="'Not well'">
                                                            <input type="radio" name="speakEnglish">
                                                            <span>Not well</span>
                                                        </label>
                                                        <label class="btn btn-default" for="speakEnglish"
                                                               ng-model="enrollment.language.speakEnglish"
                                                               ng-required="true" uib-btn-radio="'Not at all'">
                                                            <input type="radio" name="speakEnglish">
                                                            <span>Not at all</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div class="form-group">
                                                    <div class="btn-group btn-group-sm" data-toggle="buttons">
                                                        <p><label class="clear">Are you of Aboriginal or Torres Strait
                                                                Islander origin?</label></p>
                                                        <label class="btn btn-default" for="aboriginal"
                                                               ng-model="enrollment.language.aboriginal"
                                                               ng-required="true" uib-btn-radio="'No'">
                                                            <input type="radio" name="aboriginal"> <span>No</span>
                                                        </label>
                                                        <label class="btn btn-default" for="aboriginal"
                                                               ng-model="enrollment.language.aboriginal"
                                                               ng-required="true" uib-btn-radio="'Yes, Aboriginal'">
                                                            <input type="radio" name="aboriginal">
                                                            <span>Yes, Aboriginal</span>
                                                        </label>
                                                        <label class="btn btn-default" for="aboriginal"
                                                               ng-model="enrollment.language.aboriginal"
                                                               ng-required="true"
                                                               uib-btn-radio="'Yes, Torres Strait Islander'">
                                                            <input type="radio" name="aboriginal"> <span>Yes, Torres Strait Islander</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                    <!-- Langugage and cultural diversity END's Here -->

                                    <!-- Disability Section STARTS Here-->
                                    <fieldset>
                                        <h2 class="page-header">Disability</h2>

                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                                <div class="form-group">
                                                    <div class="btn-group btn-group-sm" data-toggle="buttons">
                                                        <p><label class="clear">Do you consider yourself to have a
                                                                disability, impairment or long-term condition?</label>
                                                        </p>
                                                        <label class="btn btn-default" for="disability"
                                                               data-target="#DisabilityCollapse"
                                                               ng-model="enrollment.language.disability"
                                                               ng-required="true" uib-btn-radio="1">
                                                            <input type="radio" name="disability"> <span>Yes</span>
                                                        </label>
                                                        <label class="btn btn-default" for="disability"
                                                               data-parent="#accordion"
                                                               data-target="#DisabilityCollapse"
                                                               ng-model="enrollment.language.disability"
                                                               ng-required="true" uib-btn-radio="0">
                                                            <input type="radio" name="diabilisty"> <span>No</span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- Disability Options STARTS Here-->
                                            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                                <div id="DisabilityCollapse" class=""
                                                     ng-show="enrollment.language.disability == '0' ">
                                                    <div class="disability-options">
                                                        <p>Please select from the list below the area(s) of disability,
                                                            impairment or long-term condition (select as many as
                                                            applicable):</p>
                                                    </div>
                                                    <div class="row">
